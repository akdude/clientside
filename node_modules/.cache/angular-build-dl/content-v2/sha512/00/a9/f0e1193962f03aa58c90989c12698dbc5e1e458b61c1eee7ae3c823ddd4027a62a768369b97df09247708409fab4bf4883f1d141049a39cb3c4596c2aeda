class AdvertisementsListComponent {
    constructor(api, route, actRoute, dialog, toastr) {
        this.api = api;
        this.route = route;
        this.actRoute = actRoute;
        this.dialog = dialog;
        this.toastr = toastr;
        this.checked = false;
        this.advertisements = [];
        this.loading = true;
        this.isDeleting = false;
        this.isError = false;
        this.errorMessage = "Sorry, Something went wrong";
        this.selectedAdvertisment = new src_app_models_advertisement__WEBPACK_IMPORTED_MODULE_1__["Advertisement"]();
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_4__["paginationConfig"];
    }
    ngOnInit() {
        this.advertisements = [];
        this.getAdvertisments();
    }
    getLoadingView() {
        this.loading = true;
    }
    getAdvertisments() {
        this.loading = true;
        let json = {
            "idAdCategory": "",
            "idLocation": "",
            "idUser": "",
            "searchKeyword": ""
        };
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementList, json).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.advertisements = data.data;
            this.paginationConfig.totalItems = this.advertisements.length;
        }, error => {
            this.loading = false;
            this.isError = true;
        });
    }
    showDeleteModal(advertisementId) {
        if (this.advertisements.find(a => a.idAdvertisement == advertisementId)) {
            const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
                width: '400px',
                height: '170px',
                data: {
                    message: 'Are you sure, you want to delete?'
                }
            });
            deleteDialog.afterClosed().subscribe(data => {
                if (data)
                    this.deleteAdverisement(advertisementId);
            });
        }
    }
    deleteAdverisement(advertisementId) {
        if (this.advertisements.find(a => a.idAdvertisement == advertisementId)) {
            this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementDelete(advertisementId)).pipe()
                .subscribe((data) => {
                this.loading = false;
                this.toastr.success('Advertisement deleted successfully', 'Deleted');
                let index = this.advertisements.findIndex(a => a.idAdvertisement == advertisementId);
                if (index >= -1) {
                    this.advertisements.splice(index, 1);
                }
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
    }
    change(e, advertisementId, nature) {
        let element = document.querySelector(".ad_status_" + advertisementId);
        let status = element.getAttribute('data-status');
        if (status == '0') {
            alert('00000');
        }
        else {
            alert(1111);
        }
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: (nature == true) ? 'Are you sure, you want to activate the advertisement?' : 'Are you sure, you want to deactivate the advertisement?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.activateAdvertisement(advertisementId, nature);
            }
            else {
                if (nature == true) {
                    e.source.checked = false;
                }
                else {
                    e.source.checked = true;
                }
            }
        });
    }
    activateAdvertisement(advertisementId, nature) {
        document.getElementById('ad_' + advertisementId).classList.remove('hide');
        let data = {
            "isActive": nature
        };
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementActivate(advertisementId), data).pipe()
            .subscribe((data) => {
            let response = nature == true ? 'Advertisement Activated successfully' : 'Advertisement Deactivated successfully';
            this.toastr.success(response, 'Done');
            this.getAdvertisments();
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.isError = true;
        });
    }
    bumpAd(idAdvertisement) {
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementBump(idAdvertisement)).subscribe((data) => {
            this.loading = false;
            this.toastr.success('Advertisement bumped successfully');
        }, error => {
            this.loading = false;
        });
    }
}
