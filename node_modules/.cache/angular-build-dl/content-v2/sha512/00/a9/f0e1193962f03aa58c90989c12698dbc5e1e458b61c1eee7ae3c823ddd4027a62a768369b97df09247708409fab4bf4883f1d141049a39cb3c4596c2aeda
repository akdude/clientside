class LocationsEditComponent {
    constructor(formBuilder, api, route, actRoute) {
        this.formBuilder = formBuilder;
        this.api = api;
        this.route = route;
        this.actRoute = actRoute;
        this.submitted = false;
        this.isError = false;
        this.loading = true;
        this.isSaving = false;
        this.errorMessage = "";
        this.loadingLocations = false;
        this.locations = [];
    }
    ngOnInit() {
        this.loading = true;
        this.location = new src_app_models_location__WEBPACK_IMPORTED_MODULE_2__["Location"]();
        this.actRoute.paramMap.subscribe(params => {
            this.location.idLocation = params.get('id');
            this.getLocation();
            this.getLocations();
        });
    }
    getLocations() {
        this.loadingLocations = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].locationList).pipe()
            .subscribe((data) => {
            this.loadingLocations = false;
            this.locations = data.data;
        }, error => {
            this.loadingLocations = false;
        });
    }
    getLocation() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].locationGet(this.location.idLocation)).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.location = data.data;
            this.locationForm = this.formBuilder.group({
                locationName: [this.location.locationName, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
                parent: [this.location.parent, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
            });
        }, error => {
            this.loading = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong';
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.locationForm.controls; }
    onSubmit() {
        this.submitted = true;
        if (this.locationForm.invalid) {
            return;
        }
        this.isError = false;
        this.isSaving = true;
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].locationUpdate(this.location.idLocation), this.locationForm.getRawValue()).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.route.navigate(['/locations/list']);
        }, (error) => {
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong.';
        });
    }
}
