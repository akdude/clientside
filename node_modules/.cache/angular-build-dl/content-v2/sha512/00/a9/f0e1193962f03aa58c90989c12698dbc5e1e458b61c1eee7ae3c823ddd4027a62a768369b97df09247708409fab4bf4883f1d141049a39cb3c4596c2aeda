class AdvertisementEditComponent {
    constructor(api, formBuilder, route, actRoute, toastr) {
        this.api = api;
        this.formBuilder = formBuilder;
        this.route = route;
        this.actRoute = actRoute;
        this.toastr = toastr;
        this.submitted = false;
        this.isError = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.loadingLocations = false;
        this.loadingCategories = false;
        this.loadingCatParams = false;
        this.loading = true;
        this.adminUser = new src_app_models_admin_user__WEBPACK_IMPORTED_MODULE_3__["AdminUser"]();
        this.advertisement = new src_app_models_advertisement__WEBPACK_IMPORTED_MODULE_5__["Advertisement"]();
        this.mediaFiles = [];
        this.fetchedMediaFiles = [];
        this.deletedMediaFiles = [];
        this.addedMediaFiles = [];
        this.categoryParams = [];
        this.categoryParamOptions = [];
        this.selectedParamOptions = [];
    }
    ngOnInit() {
        this.getLocations();
        this.getCategories();
        this.actRoute.paramMap.subscribe(params => {
            this.advertisement.idAdvertisement = params.get('id');
            this.getAdvertisement();
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.advertismentForm.controls; }
    getAdvertisement() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementGet(this.advertisement.idAdvertisement)).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.advertisement = data.data;
            this.mediaFiles = data.data.media;
            // as javascript only hold a reference to an object, not the object itself.
            // so if we change this.mediaFiles then its changes also affects on this.fetchedMediaFiles.
            // to prevent this, assign array with .slice(0).
            this.fetchedMediaFiles = data.data.media.length > 0 ? data.data.media.slice(0) : data.data.media;
            this.setForm();
            this.onChangeCategory(this.advertisement.idAdCategory);
        }, error => {
            this.loading = false;
            this.isError = true;
            this.errorMessage = 'Sorry, something went wrong';
        });
    }
    setForm() {
        this.advertismentForm = this.formBuilder.group({
            idUser: [this.adminUser.get().idUser, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            idAdCategory: [this.advertisement.idAdCategory, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            idLocation: [this.advertisement.idLocation, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            adTitle: [this.advertisement.adTitle, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            adDesc: [this.advertisement.adDesc, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            price: [this.advertisement.price, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
        });
    }
    getLocations() {
        this.loadingLocations = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].locationList).pipe()
            .subscribe((data) => {
            this.loadingLocations = false;
            this.locations = data.data;
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.loadingLocations = false;
            this.isError = true;
        });
    }
    getCategories() {
        this.loadingCategories = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryList).pipe()
            .subscribe((data) => {
            this.loadingCategories = false;
            this.categories = data.data;
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.loadingCategories = false;
            this.isError = true;
        });
    }
    onSubmit() {
        this.submitted = true;
        // if (this.advertismentForm.invalid) {
        //   return;
        // }
        this.isError = false;
        this.isSaving = true;
        let data = this.advertismentForm.getRawValue();
        debugger;
        if (this.selectedParamOptions.length > 0) {
            for (let item of this.selectedParamOptions) {
                data[item.paramName] = item.options.join(',');
            }
        }
        data.media = this.fetchedMediaFiles;
        for (let f = 0; f < this.fetchedMediaFiles.length; f++) {
            let i = this.deletedMediaFiles.findIndex(a => a.idAdGallery == this.fetchedMediaFiles[f].idAdGallery);
            if (i > -1) {
                data.media[f].image = "";
            }
            else {
                data.media[f].image = this.fetchedMediaFiles[f].fileName;
            }
        }
        this.addedMediaFiles = Object(src_app_constants__WEBPACK_IMPORTED_MODULE_4__["getMediaFiles"])();
        if (this.addedMediaFiles.length > 0) {
            data.media.push(...this.addedMediaFiles);
        }
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementUpdate(this.advertisement.idAdvertisement), data).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.toastr.success('Advertisement updated successfully', 'Done');
            this.route.navigate(['/advertisements/list']);
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
        });
    }
    deleteMedial(idAdGallery) {
        let media = this.mediaFiles.find(a => a.idAdGallery == idAdGallery);
        if (media) {
            this.mediaFiles.splice(this.mediaFiles.findIndex(a => a.idAdGallery == idAdGallery), 1);
            this.deletedMediaFiles.push(media);
        }
    }
    editMedia(fileEvent, media) {
        var reader = new FileReader();
        let mediaFile = new src_app_models_media_file__WEBPACK_IMPORTED_MODULE_6__["MediaFile"]();
        //Get base64
        reader.onload = (event) => {
            mediaFile.base64 = event.target.result.toString();
            mediaFile.image = mediaFile.base64;
            let index = this.mediaFiles.findIndex(a => a.idAdCategory == media.idAdCategory);
            if (index > -1) {
                this.mediaFiles[index].fileName = mediaFile.base64;
            }
        };
        let base64 = reader.readAsDataURL(fileEvent.target.files[0]);
        mediaFile.size = fileEvent.target.files[0].size;
        mediaFile.name = fileEvent.target.files[0].name;
    }
    onChangeCategory(idAdCategory = null) {
        if (this.f.idAdCategory.value || idAdCategory) {
            let idAdCat = this.f.idAdCategory.value || idAdCategory;
            this.loadingCatParams = true;
            this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].getCategoryParams(idAdCat), {}).pipe()
                .subscribe((res) => {
                this.loadingCatParams = false;
                this.categoryParams = res.data;
                if (this.categoryParams.length > 0) {
                    this.categoryParamOptions = [];
                    for (let i = 0; i < this.categoryParams.length; i++) {
                        if ((this.categoryParams[i].paramType == src_app_enums__WEBPACK_IMPORTED_MODULE_7__["CategoryParamTypes"].Options || this.categoryParams[i].paramType == src_app_enums__WEBPACK_IMPORTED_MODULE_7__["CategoryParamTypes"].Radio) && this.categoryParams[i].defaultValueArray.length > 0) {
                            let options = [];
                            this.categoryParams[i].defaultValueArray.forEach(element => {
                                options.push({ 'text': element, 'isChecked': false });
                            });
                            this.categoryParamOptions.push({ 'paramName': this.categoryParams[i].paramName,
                                options: options
                            });
                            this.categoryParams[i].defaultValueArray = options;
                            this.selectedParamOptions.push({ 'paramName': this.categoryParams[i].paramName, options: [] });
                        }
                        this.advertismentForm.addControl(this.categoryParams[i].paramName, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                    }
                }
                else {
                    this.setForm();
                    this.selectedParamOptions = [];
                }
            }, (error) => {
                this.loadingCatParams = false;
            });
        }
    }
    changeParamOptionSelection(event, item, paramName) {
        for (let option of this.selectedParamOptions) {
            if (option.paramName == paramName) {
                if (event.currentTarget.checked) {
                    option.options.push(item.text);
                }
                else {
                    option.options.splice(item.text, 1);
                }
            }
        }
    }
}
