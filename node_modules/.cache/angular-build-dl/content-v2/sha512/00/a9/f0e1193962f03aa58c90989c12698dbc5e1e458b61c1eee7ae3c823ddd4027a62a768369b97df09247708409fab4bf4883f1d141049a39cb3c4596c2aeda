class BadgeconfigAddComponent {
    constructor(toastr, _fb, router, _api, _ac) {
        this.toastr = toastr;
        this._fb = _fb;
        this.router = router;
        this._api = _api;
        this._ac = _ac;
        this.actionType = 'Add';
        this.title = 'Create New BadgeConfig';
        this.submitted = false;
        this.loading = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.idBadgeConfig = '';
        this.badgeData = '';
        this.image = '';
    }
    ngOnInit() {
        this.createBadgeFormInit();
        this.getbadgeList();
        this._ac.params.subscribe(params => {
            this.idBadgeConfig = params.id;
            if (this.idBadgeConfig) {
                console.log(this.idBadgeConfig);
                this.getBadge();
            }
        });
    }
    //Initializing Badgeform for validation
    createBadgeFormInit() {
        this.badgeForm = this._fb.group({
            'title': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'slug': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'type': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'description': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'parents': [0],
            'price': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'image': ['', this.actionType == 'Add' ? [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required] : []],
            'visibility': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]]
        });
    }
    get f() { return this.badgeForm.controls; }
    //Calling Submit function
    onSubmit() {
        console.log(this.badgeForm.value);
        this.submitted = true;
        //Checking form validaton
        if (this.badgeForm.valid) {
            this.isSaving = true;
            //Creating BadgeConfig
            if (this.actionType == 'Add') {
                let data = {
                    title: this.f.title.value,
                    slug: this.f.slug.value,
                    type: this.f.type.value,
                    description: this.f.description.value,
                    price: this.f.price.value,
                    image: this.f.image.value,
                    visibility: this.f.visibility.value
                };
                if (this.f.parents.value) {
                    data.parents = (this.f.parents.value).join();
                }
                this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].badgeCreate, data).subscribe((data) => {
                    console.log(data);
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('BadgeConfig  created successfully');
                    this.router.navigate(['badge-config/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, Something went wrong';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
            //Editing BadgeConfig details
            if (this.actionType == 'Edit') {
                let data = {
                    title: this.f.title.value,
                    slug: this.f.slug.value,
                    type: this.f.type.value,
                    description: this.f.description.value,
                    price: this.f.price.value,
                    visibility: this.f.visibility.value
                };
                this.f.parents.value ? (data.parents = (this.f.parents.value).join()) : '';
                this.f.image.value ? (data.image = this.f.image.value) : '';
                console.log(data);
                this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].badgeUpdate(this.idBadgeConfig), data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('BadgeConfig  updated successfully');
                    this.router.navigate(['badge-config/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
        }
        else {
            console.log('error');
            return;
        }
    }
    //Getting BadgeConfig data using (idBadgeConfig)
    getBadge() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].badgeGet(this.idBadgeConfig)).subscribe((data) => {
            console.log(data);
            this.badgeData = data.data;
            if (this.badgeData) {
                this.loading = false;
                this.setbadgeData(this.badgeData);
            }
        });
    }
    //Setting BadgeConfig data in edit form
    setbadgeData(data) {
        this.badgeForm.patchValue({
            'title': data.title,
            'slug': data.slug,
            'type': data.type,
            'description': data.description,
            'price': data.price,
            'parents': data.parents,
            'visibility': data.visibility
        });
        this.image = data.imageLink;
    }
    //Gettting BadgeConfig list
    getbadgeList() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].badgeList).subscribe((data) => {
            this.loading = false;
            this.badgeList = data.data;
        }, error => {
            this.loading = false;
        });
    }
    //Cancel BadgeConfig
    cancelbadgeConfigcreate() {
        this.router.navigate(['badge-config/list']);
    }
    //Function for uploading image
    handleInputChange(e) {
        var file = e.dataTransfer ? e.dataTransfer.files[0] : e.target.files[0];
        console.log(file);
        var pattern = /image-*/;
        var reader = new FileReader();
        if (!file.type.match(pattern)) {
            alert('invalid format');
            return;
        }
        reader.onload = this._handleReaderLoaded.bind(this);
        reader.readAsDataURL(file);
    }
    _handleReaderLoaded(e) {
        let reader = e.target;
        this.image = reader.result;
        console.log(this.image);
        this.badgeForm.patchValue({ image: this.image });
        console.log(this.image);
    }
    cancel() {
        this.image = "null";
    }
}
