class CategoryListComponent {
    constructor(toastr, api, dialog, _utils) {
        this.toastr = toastr;
        this.api = api;
        this.dialog = dialog;
        this._utils = _utils;
        this.categoryList = [];
        this.selectedCategoryList = new src_app_models_category__WEBPACK_IMPORTED_MODULE_1__["CategoryList"]();
        this.loading = true;
        this.errorMessage = "Sorry, Something went wrong";
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_3__["paginationConfig"];
    }
    ngOnInit() {
        this.getCategoryList();
        console.log(this.paginationConfig);
    }
    //Getting Category list
    getCategoryList() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_4__["apiUrls"].categoryList)
            .subscribe((data) => {
            this.loading = false;
            this.categoryList = data.data;
            this.paginationConfig.totalItems = this.categoryList.length;
        }, error => {
            this.loading = false;
        });
    }
    //Deleting Category using (idAdCategory)
    confirmDelete(res) {
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_2__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data)
                this.deleteCategory(res);
        });
    }
    deleteCategory(res) {
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_4__["apiUrls"].categoryDelete(res.idAdCategory)).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.toastr.success('Category  deleted successfully', "Deleted");
            this.deletecategoryFromList(res);
        }, error => {
        }, () => { });
    }
    deletecategoryFromList(res) {
        this.categoryList = this._utils.filterArrayObj(this.categoryList, 'idAdCategory', res.idAdCategory);
    }
}
