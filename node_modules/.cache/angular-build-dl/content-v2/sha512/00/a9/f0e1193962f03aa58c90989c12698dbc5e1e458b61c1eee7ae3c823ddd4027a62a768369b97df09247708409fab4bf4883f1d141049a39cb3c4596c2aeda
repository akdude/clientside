class BannerListComponent {
    constructor(_api, toastr, dialog, _utils) {
        this._api = _api;
        this.toastr = toastr;
        this.dialog = dialog;
        this._utils = _utils;
        this.loading = true;
        this.bannerList = [];
        this.isDeleting = false;
        this.isError = false;
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_1__["paginationConfig"];
    }
    ngOnInit() {
        this.getbannerList();
    }
    //Getting Bannerlist
    getbannerList() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].bannerList).subscribe((data) => {
            this.loading = false;
            this.bannerList = data.data;
            this.paginationConfig.totalItems = this.bannerList.length;
        }, (error) => {
            this.loading = false;
            this.isError = true;
        });
    }
    //Deleting Banner using (idBanner)
    confirmDelete(res) {
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure want to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data)
                this.deleteBanner(res);
        });
    }
    deleteBanner(res) {
        this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].bannerDelete(res.idBanner)).subscribe((data) => {
            this.isDeleting = false;
            this.toastr.success('Banner deleted successfully', "Daleted");
            this.deletebannerFromList(res);
        }, error => {
        }, () => { });
    }
    deletebannerFromList(res) {
        this.bannerList = this._utils.filterArrayObj(this.bannerList, 'idBanner', res.idBanner);
    }
    //Function for toggle button
    toggleActive(banner, e) {
        let status = e.checked ? 'true' : 'false';
        let active = e.checked ? '0' : '1';
        const activeDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure to ' + status + ' the ' + banner.bannerName + '?'
            }
        });
        activeDialog.afterClosed().subscribe(data => {
            (data) ? 'this.toggleActiveBanner(user.id, e)' : this.bannerList = this._utils.changeArrayObjProp(this.bannerList, 'id', banner.id, 'false', !e.checked);
        });
    }
    toggleActiveBanner(id, e) {
        let data = {
            id: id,
            active: e.checked ? 1 : 0
        };
        this.bannerList = this._utils.changeArrayObjProp(this.bannerList, 'id', id, 'false', e.checked);
        this._api.putApiData('this._urls.bannerActiveUrl', data).subscribe((data) => {
            let id = data.data.id;
            this.bannerList = this._utils.changeArrayObjProp(this.bannerList, 'id', id, 'false', e.checked);
        }, error => {
        }, () => {
        });
    }
}
