class AppComponent {
    constructor(settingsService, router) {
        this.settingsService = settingsService;
        this.router = router;
        this.fixedSidebar = false;
        this.smallSidebar = false;
        this.settingsOpen = false;
        this.sidebarColor = 'bg-black-300';
        this.colors = ["blue", "default", "gray", "green", "red", "sky-blue", "yellow"];
        this.sizes = ["mini", "small", "normal", "large"];
        this.onText = "On";
        this.offText = "Off";
        this.onColor = "green";
        this.offColor = "red";
        this.size = "normal";
        // For Sidebar BG
        this.bgBlack300 = true;
        this.bgPrimary = false;
        this.bgLight = false;
        this.bgDanger = false;
        this.bgSuccess = false;
        this.bgWarning = false;
        this.isLoginPage = false;
    }
    ngOnInit() {
        if (window.location.pathname == "/accounts/login") {
            this.isLoginPage = true;
        }
        else {
            this.fixedSidebar = this.settingsService.getFixedSidebar();
            this.innerWidth = (window.innerWidth);
            this.router.events.subscribe((evt) => {
                if (!(evt instanceof _angular_router__WEBPACK_IMPORTED_MODULE_2__["NavigationEnd"])) {
                    return;
                }
                // console.log(this.innerWidth);
                if (this.innerWidth < 991) {
                    jquery__WEBPACK_IMPORTED_MODULE_1__('.left-sidebar').toggle();
                }
                window.scrollTo(0, 0);
            });
            // Toggle user info on sidebar
            jquery__WEBPACK_IMPORTED_MODULE_1__('.user-info-handle').on('click', function (event) {
                event.preventDefault();
                jquery__WEBPACK_IMPORTED_MODULE_1__('.user-info').toggleClass('closed');
            });
            // Toggle small sidebar
            jquery__WEBPACK_IMPORTED_MODULE_1__('.small-nav-handle').on('click', function (event) {
                event.preventDefault();
                jquery__WEBPACK_IMPORTED_MODULE_1__('.left-sidebar').toggleClass('small-nav');
                jquery__WEBPACK_IMPORTED_MODULE_1__('.navbar-header').toggleClass('small-nav-header');
            });
            // Toggle Mobile Nav
            jquery__WEBPACK_IMPORTED_MODULE_1__('.mobile-nav-toggle').on('click', function (event) {
                event.preventDefault();
                jquery__WEBPACK_IMPORTED_MODULE_1__('.left-sidebar').toggle();
            });
            // Toggle sidebar dropdown
            jquery__WEBPACK_IMPORTED_MODULE_1__('.left-sidebar .has-children').not('.open').find('.child-nav').slideUp('100');
            jquery__WEBPACK_IMPORTED_MODULE_1__('.left-sidebar .has-children>a').on('click', function (event) {
                event.preventDefault();
                jquery__WEBPACK_IMPORTED_MODULE_1__(this).parent().toggleClass('open');
                jquery__WEBPACK_IMPORTED_MODULE_1__(this).parent().find('.child-nav').slideToggle('500');
            });
        }
    }
}
