    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("ngIf", ctx_r1.isSaving);
} }
class UserAddComponent {
    constructor(_ac, _fb, _api, _router, toastr) {
        this._ac = _ac;
        this._fb = _fb;
        this._api = _api;
        this._router = _router;
        this.toastr = toastr;
        this.actionType = 'Add';
        this.title = 'Add New User';
        this.idUser = '';
        this.submitted = false;
        this.isError = false;
        this.isAdd = true;
        this.loading = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.userData = '';
    }
    ngOnInit() {
        this.createUserFormInit();
        this.getusertypeList();
        this._ac.params.subscribe(params => {
            this.idUser = params.id;
            if (this.idUser) {
                this.isAdd = false;
                this.getUser();
            }
        });
    }
    //Initializing Userform for validation
    createUserFormInit() {
        this.userForm = this._fb.group({
            'idUserType': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'userName': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'email': ['', this.actionType == 'Add' ? [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required] : []],
            'password': ['', this.actionType == 'Add' ? [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required] : []]
        });
    }
    get f() { return this.userForm.controls; }
    //Calling Submit function
    onSubmit() {
        this.submitted = true;
        //Checking form validaton
        if (this.userForm.valid) {
            this.isSaving = true;
            //Creating Users
            if (this.actionType == 'Add') {
                this.isError = false;
                let data = {
                    idUserType: this.f.idUserType.value,
                    userName: this.f.userName.value,
                    email: this.f.email.value,
                    password: this.f.password.value
                };
                this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userCreate, data).subscribe((data) => {
                    console.log(data);
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('User created successfully', "Created");
                    this._router.navigate(['user/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                    this.isError = true;
                });
            }
            //Editing Users details
            if (this.actionType == 'Edit') {
                let data = {
                    idUserType: this.f.idUserType.value,
                    userName: this.f.userName.value
                };
                this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userUpdate(this.idUser), data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('User Updated successfully', "Updated");
                    this._router.navigate(['user/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                    this.isError = true;
                });
            }
        }
        else {
            return;
        }
    }
    //Getting User data using (idUser)
    getUser() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userGet(this.idUser)).subscribe((data) => {
            console.log(data);
            this.userData = data.data;
            if (this.userData) {
                this.loading = false;
                this.setuserData(this.userData);
            }
        });
    }
    //Setting User data in edit form
    setuserData(data) {
        this.userForm.patchValue({
            'idUserType': data.idUserType,
            'userName': data.userName,
            'email': data.email
        });
    }
    //Getting Usertypelist
    getusertypeList() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userTypeList).subscribe((data) => {
            this.loading = false;
            this.usertypeList = data.data;
        }, (error) => {
            this.loading = false;
        });
    }
    //Cancel User
    cancelUsercreate() {
        this._router.navigate(['user/list']);
    }
}
