class LocationsListComponent {
    constructor(dialog, api, toastr) {
        this.dialog = dialog;
        this.api = api;
        this.toastr = toastr;
        this.loading = true;
        this.isDeleting = false;
        this.isError = false;
        this.errorMessage = "Sorry, Something went wrong";
        this.selectedLocation = new src_app_models_location__WEBPACK_IMPORTED_MODULE_1__["Location"]();
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_4__["paginationConfig"];
    }
    ngOnInit() {
        this.locations = [];
        this.getLocations();
    }
    getLocations() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].locationList).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.locations = data.data;
            this.paginationConfig.totalItems = this.locations.length;
        }, error => {
            this.loading = false;
            this.isError = true;
        });
    }
    showDeleteModal(location) {
        this.isDeleting = false;
        // let inputElement: HTMLElement = this.deleteModalButton.nativeElement as HTMLElement;
        // inputElement.click();
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure, you want to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.selectedLocation = location;
                this.delete();
            }
        });
    }
    delete() {
        if (this.selectedLocation) {
            this.isDeleting = true;
            this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].locationDelete(this.selectedLocation.idLocation)).pipe()
                .subscribe((data) => {
                this.isDeleting = false;
                // let inputElement: HTMLElement = this.cancelModalButton.nativeElement as HTMLElement;
                // inputElement.click();
                this.toastr.success('Location deleted successfully', "Deleted");
                let index = this.locations.indexOf(this.selectedLocation);
                if (index >= -1) {
                    this.locations.splice(index, 1);
                }
                this.selectedLocation = new src_app_models_location__WEBPACK_IMPORTED_MODULE_1__["Location"]();
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
        else {
            alert('Location not found');
        }
    }
}
