    }
    postApiData(url, jsonData = {}, authenticateAdmin = true) {
        if (authenticateAdmin) {
            return this._http.post(src_app_api_urls__WEBPACK_IMPORTED_MODULE_5__["apiUrls"].getSessionKeyUrl, this._adminUser).pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_2__["mergeMap"])((data) => {
                jsonData.sessionKey = data.data.sessionKey;
                jsonData.publicKey = this._adminUser.publicKey;
                jsonData.accessKey = this._adminUser.accessKey;
                return this._http.post(url, jsonData, this._httpOptions);
            }), (Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_2__["catchError"])(err => {
                this.apiError(err);
                return Object(rxjs__WEBPACK_IMPORTED_MODULE_6__["throwError"])(err);
            })));
        }
