class BadgeconfigListComponent {
    constructor(toastr, _api, dialog, _utils) {
        this.toastr = toastr;
        this._api = _api;
        this.dialog = dialog;
        this._utils = _utils;
        this.badgeList = [];
        this.loading = true;
        this.isDeleting = false;
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_2__["paginationConfig"];
    }
    ngOnInit() {
        this.getbadgeList();
    }
    //Getting BadgeConfiglist
    getbadgeList() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].badgeList).subscribe((data) => {
            this.loading = false;
            this.badgeList = data.data;
            this.paginationConfig.totalItems = this.badgeList.length;
        }, error => {
            this.loading = false;
        });
    }
    //Deleting BadgeConfig using (idBadgeConfig)
    confirmDelete(res) {
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_1__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data)
                this.deleteBadge(res);
        });
    }
    deleteBadge(res) {
        this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].badgeDelete(res.idBadgeConfig))
            .subscribe((data) => {
            this.isDeleting = false;
            this.toastr.success('BadgeConfig  deleted successfully', "Deleted");
            this.deletebadgeFromList(res);
        }, error => {
        }, () => { });
    }
    deletebadgeFromList(res) {
        this.badgeList = this._utils.filterArrayObj(this.badgeList, 'idBadgeConfig', res.idBadgeConfig);
    }
}
