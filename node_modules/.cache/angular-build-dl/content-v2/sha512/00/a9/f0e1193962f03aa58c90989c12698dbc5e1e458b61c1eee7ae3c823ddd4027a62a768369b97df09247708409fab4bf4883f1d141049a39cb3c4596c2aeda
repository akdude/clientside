class LoginComponent {
    constructor(formBuilder, router, accountsService, localStorage) {
        this.formBuilder = formBuilder;
        this.router = router;
        this.accountsService = accountsService;
        this.localStorage = localStorage;
        this.submitted = false;
        this.isError = false;
        this.userData = [];
    }
    ngOnInit() {
        this.loginForm = this.formBuilder.group({
            email: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            password: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.loginForm.controls; }
    onSubmit() {
        this.submitted = true;
        if (this.loginForm.invalid) {
            return;
        }
        this.isError = false;
        this.accountsService.login({ email: this.f.email.value, password: this.f.password.value }).pipe()
            .subscribe((data) => {
            this.submitted = false;
            localStorage.setItem(src_app_enums__WEBPACK_IMPORTED_MODULE_2__["LocalStorageKeys"].AdminUser, JSON.stringify(data.data));
            // should refresh the page while redirecting
            window.location.href = '/dashboard';
        }, error => {
            this.submitted = false;
            this.isError = true;
        });
    }
