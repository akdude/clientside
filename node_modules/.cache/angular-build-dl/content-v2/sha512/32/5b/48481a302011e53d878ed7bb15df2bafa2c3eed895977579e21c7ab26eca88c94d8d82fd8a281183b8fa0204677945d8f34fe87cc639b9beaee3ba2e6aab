class AdvertisementEditComponent {
    constructor(api, formBuilder, route, actRoute, toastr) {
        this.api = api;
        this.formBuilder = formBuilder;
        this.route = route;
        this.actRoute = actRoute;
        this.toastr = toastr;
        this.Editor = _ckeditor_ckeditor5_build_classic__WEBPACK_IMPORTED_MODULE_5__;
        this.submitted = false;
        this.isError = false;
        this.errorMessage = "";
        this.adid = "";
        this.isSaving = false;
        this.loading = false;
        this.loadingLocations = false;
        this.loadingCategories = false;
        this.offervalue = false;
        this.wantvalue = false;
        this.categoryParams = [];
        this.allcontrols = [];
        this.mediaFiles = [];
        this.adData = [];
        this.image_ids = [];
        this.image_urls = [];
        this.deletedMediaFiles = [];
        this.categoryParamOptions = [];
        this.fetchedMediaFiles = [];
        this.selectedParamOptions = [];
        this.loadingCatParams = false;
        this.adminUser = new src_app_models_admin_user__WEBPACK_IMPORTED_MODULE_3__["AdminUser"]();
        this.advertisement = new src_app_models_advertisement__WEBPACK_IMPORTED_MODULE_4__["Advertisement"]();
    }
    ngOnInit() {
        this.getLocations();
        this.getCategories();
        this.actRoute.paramMap.subscribe(params => {
            var ad_id = params.get('id');
            this.adid = ad_id;
            this.getAdvertisement(ad_id);
        });
        this.setForm();
    }
    ngAfterViewChecked() {
        this.strip();
    }
    strip() {
        var elements = document.getElementsByClassName('parsecontent');
        if (elements) {
            for (let i = 0; i < elements.length; i++) {
                elements[i].innerHTML = elements[i].innerText.replace(/(<([^>]+)>)/gi, "");
            }
        }
    }
    getAdvertisement(ad_id) {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementGet(ad_id)).pipe()
            .subscribe((data) => {
            let params = data.data.additionalParams;
            for (let i = 0; i < params.length; i++) {
                for (let j = 0; j < params[i]['params'].length; j++) {
                    var element = params[i]['params'][j]['inputName'];
                    var isrequired = params[i]['params'][j]['isRequired'];
                    var isimage = params[i]['params'][j]['paramType'];
                    if (params[i]['params'][j]['paramType'] == 'radio') {
                        var element = params[i]['params'][j]['inputName'];
                        var isrequired = params[i]['params'][j]['isRequired'];
                        var options = params[i]['params'][j]['values'];
                        if (isrequired) {
                            for (let z = 0; z < options.length; z++) {
                                if (options[z]['checked'] == "true") {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](options[z]['value'], _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                                    this.advertismentForm.controls[element].setValue(options[z]['value']);
                                }
                                else {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                                }
                            }
                        }
                        else {
                            for (let z = 0; z < options.length; z++) {
                                if (options[z]['checked'] == "true") {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](options[z]['value']));
                                    this.advertismentForm.controls[element].setValue(options[z]['value']);
                                }
                                else {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''));
                                }
                            }
                        }
                    }
                    if (params[i]['params'][j]['paramType'] == 'options') {
                        var element = params[i]['params'][j]['inputName'];
                        var isrequired = params[i]['params'][j]['isRequired'];
                        var options = params[i]['params'][j]['values'];
                        if (isrequired) {
                            for (let z = 0; z < options.length; z++) {
                                if (options[z]['selected'] == "true") {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](options[z]['value'], _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                                    this.advertismentForm.controls[element].setValue(options[z]['value']);
                                }
                                else {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                                }
                            }
                        }
                        else {
                            for (let z = 0; z < options.length; z++) {
                                if (options[z]['selected'] == "true") {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](options[z]['value']));
                                    this.advertismentForm.controls[element].setValue(options[z]['value']);
                                }
                                else {
                                    this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](''));
                                }
                            }
                        }
                    }
                    if (isimage == 'checkbox') {
                        if (isrequired) {
                            this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](params[i]['params'][j]['values'][0]['value'], _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required));
                        }
                        else {
                            this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](params[i]['params'][j]['values'][0]['value']));
                        }
                        this.advertismentForm.controls[element].setValue(params[i]['params'][j]['values'][0]['value']);
                    }
                    if (isimage == 'text') {
                        if (isrequired) {
                            this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](params[i]['params'][j]['values'][0]['value'], [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].pattern(params[i]['params'][j]['values'][0]['regex'])]));
                        }
                        else {
                            this.advertismentForm.addControl(element, new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](params[i]['params'][j]['values'][0]['value'], _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].pattern(params[i]['params'][j]['values'][0]['regex'])));
                        }
                        this.advertismentForm.controls[element].setValue(params[i]['params'][j]['values'][0]['value']);
                    }
                    if (isimage !== 'image') {
                        this.allcontrols.push(element);
                    }
                }
            }
            for (let i = 0; i < params.length; i++) {
                if (params[i].block == 'Media') {
                    var mediablockparams = params[i].params;
                    for (let j = 0; j < mediablockparams.length; j++) {
                        if (mediablockparams[j].paramType == 'image') {
                            var images = mediablockparams[j].values;
                            for (let k = 0; k < images.length; k++) {
                                this.mediaFiles[k] = [images[k].name, images[k].value, images[k].idAdGallery];
                            }
                        }
                    }
                }
            }
            this.loading = false;
            this.categoryParams = data.data.additionalParams;
            this.advertismentForm.patchValue({ 'idAdCategory': data.data.idAdCategory });
            this.advertismentForm.patchValue({ 'adTitle': data.data.adTitle });
            this.advertismentForm.patchValue({ 'adDesc': data.data.adDesc });
            if (data.data.adType == 'Offering') {
                this.offervalue = true;
                this.wantvalue = false;
            }
            else {
                this.offervalue = false;
                this.wantvalue = true;
            }
            document.getElementById('advertiser').value = data.data.advertiserType;
            this.advertismentForm.patchValue({ 'price': data.data.price });
            this.advertismentForm.patchValue({ 'idLocation': data.data.idLocation });
        }, error => {
            this.loading = false;
            this.isError = true;
            this.errorMessage = 'Sorry, something went wrong';
        });
    }
    setForm() {
        this.advertismentForm = this.formBuilder.group({
            idUser: [this.adminUser.get().idUser, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            idAdCategory: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            idLocation: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            adTitle: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            adDesc: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            price: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            adType: [(this.offervalue) ? (this.offervalue) : (this.wantvalue), _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.advertismentForm.controls; }
    getLocations() {
        this.loadingLocations = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].locationList).pipe()
            .subscribe((data) => {
            this.loadingLocations = false;
            this.locations = data.data;
        }, (error) => {
            this.errorMessage = 'Sorry, something went wrong.';
            this.loadingLocations = false;
            this.isError = true;
        });
    }
    getCategories() {
        this.loadingCategories = true;
        let data = {
            withChild: 1
        };
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryList, data).pipe()
            .subscribe((data) => {
            this.loadingCategories = false;
            this.categories = data.data;
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.loadingCategories = false;
            this.isError = true;
        });
    }
    onSelectFile(event, id, count) {
        if (event.target.files && event.target.files[0]) {
            var reader = new FileReader();
            reader.readAsDataURL(event.target.files[0]);
            reader.onload = (event) => {
                var url = event.target.result;
                this.image_ids.push(id);
                this.image_urls.push(url.toString());
                var elem = '<img id="mainimg_' + id + '" src = ' + url + ' class="c-image final_img edit_img_' + count + '"></img>' +
                    '<div class=" ml-50 mb-30">' +
                    '<button" type="button" id="remove_image_' + id + '" class="btn btn-danger icon-only ml-15 mt-10"><i class="fa fa-trash-o"></i></button>' +
                    '</div>';
                var q = document.getElementById('mainimg_' + id);
                if (q) {
                    q.remove();
                    document.getElementById('remove_image_' + id).remove();
                }
                document.getElementById('image_html_' + id).insertAdjacentHTML('afterend', elem);
                document.getElementById('remove_image_' + id).addEventListener('click', function () {
                    document.getElementById('mainimg_' + id).remove();
                    document.getElementById('remove_image_' + id).remove();
                });
                document.getElementById(id).value = "";
            };
        }
    }
    removeImage(id) {
        document.getElementById('mainimg_' + id).remove();
        document.getElementById('remove_image_' + id).remove();
        document.getElementById(id).value = "";
    }
    onSubmit() {
        this.submitted = true;
        this.isError = false;
        this.isSaving = true;
        let data = this.advertismentForm.getRawValue();
        if (this.selectedParamOptions.length > 0) {
            for (let item of this.selectedParamOptions) {
                data[item.paramName] = item.options.join(',');
            }
        }
        var elems = document.getElementsByClassName("custom-op");
        for (let i = 0; i < elems.length; i++) {
            if (!elems[i].classList.contains('mat-radio-group') && !elems[i].classList.contains('mat-checkbox')) {
                var id = elems[i].id;
                var x = elems[i].value;
                var required = elems[i].classList.contains('required');
                if (!x && required) {
                    var elem = '<span id="error_' + id + '" style="color:brown;"><i>This field is required</i></span>';
                    var elemexist = document.getElementById('error_' + id);
                    if (!elemexist) {
                        document.getElementById(id).insertAdjacentHTML('afterend', elem);
                    }
                    this.isSaving = false;
                    this.toastr.warning('Please fill in all the fields to continue', 'Wait');
                    return;
                }
                else {
                    if (document.getElementById('error_' + id)) {
                        document.getElementById('error_' + id).remove();
                    }
                }
                this.advertismentForm.controls[id].setValue(x);
                data[id] = x;
            }
            if (elems[i].classList.contains('mat-radio-group')) {
                var rid = elems[i].id;
                var req = elems[i].classList.contains('required');
                var rvalue = this.advertismentForm.get(rid).value;
                if (!rvalue && req) {
                    var elem = '<span id="error_' + rid + '" style="color:brown;"><i>This field is required</i></span>';
                    var elemexist = document.getElementById('error_' + rid);
                    if (!elemexist) {
                        document.getElementById(rid).insertAdjacentHTML('afterend', elem);
                    }
                    this.isSaving = false;
                    this.toastr.warning('Please fill in all the fields to continue', 'Wait');
                    return;
                }
                else {
                    this.advertismentForm.controls[rid].setValue(rvalue);
                    if (document.getElementById('error_' + rid)) {
                        document.getElementById('error_' + rid).remove();
                    }
                }
            }
            if (elems[i].classList.contains('mat-checkbox')) {
                if (elems[i].classList.contains('mat-checkbox-checked')) {
                    var value = "true";
                }
                else {
                    var value = "false";
                }
                var id = elems[i].id;
                this.advertismentForm.controls[id].setValue(value);
                data[id] = value;
            }
        }
        var radio = document.getElementsByClassName("mat-radio-group");
        if (radio) {
            for (let c = 0; c < radio.length; c++) {
                var id = radio[c].id;
                for (let i = 0; i < radio[c].childElementCount; i++) {
                    if (radio[c].children[i].classList.contains('mat-radio-checked')) {
                        data[id] = radio[c].children[i].attributes[2].value;
                    }
                }
            }
        }
        var mediafiles = this.mediaFiles;
        for (let l = 0; l < mediafiles.length; l++) {
            var imageid = mediafiles[l][0];
            data[imageid] = {};
            var element = document.getElementsByClassName('edit_img_' + [l]);
            if (element[0]) {
                var src = element[0].src;
                if (src == mediafiles[l][1]) {
                    data[imageid].image = mediafiles[l][1];
                    data[imageid].idAdGallery = mediafiles[l][2]; //same data with filled images
                }
                else {
                    var oldelem = mediafiles[l][1];
                    if (oldelem) {
                        data[imageid].image = src;
                        data[imageid].idAdGallery = mediafiles[l][2]; //update
                    }
                    else {
                        data[imageid].image = src;
                        data[imageid].idAdGallery = ""; //create
                    }
                }
            }
            else {
                var exist = mediafiles[l][1];
                if (exist) {
                    data[imageid].image = "";
                    data[imageid].idAdGallery = mediafiles[l][2]; //delete
                }
                else {
                    data[imageid].image = "";
                    data[imageid].idAdGallery = ""; //same data with no images
                }
            }
        }
        if (this.advertismentForm.invalid) {
            var comps = this.advertismentForm.controls;
            for (const key in comps) {
                if (Object.prototype.hasOwnProperty.call(comps, key)) {
                    const element = comps[key];
                    var id = key;
                    if (element.status == "INVALID") {
                        var elem = '<span id="error_' + id + '" style="color:brown;"><i>Invalid Input Pattern</i></span>';
                        var elemexist = document.getElementById('error_' + id);
                        if (!elemexist) {
                            document.getElementById(id).insertAdjacentHTML('afterend', elem);
                        }
                    }
                    else {
                        if (document.getElementById('error_' + id)) {
                            document.getElementById('error_' + id).remove();
                        }
                    }
                }
            }
            this.isSaving = false;
            this.toastr.warning('Please check your input', 'Wait');
            return;
        }
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].advertisementUpdate(this.adid), data).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.toastr.success('Advertisement updated successfully', 'Done');
            this.route.navigate(['/advertisements/list']);
        }, (error) => {
            this.errorMessage = 'Sorry, something went wrong.';
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
        });
    }
    deleteMedial(idAdGallery) {
        let media = this.mediaFiles.find(a => a.idAdGallery == idAdGallery);
        if (media) {
            this.mediaFiles.splice(this.mediaFiles.findIndex(a => a.idAdGallery == idAdGallery), 1);
            this.deletedMediaFiles.push(media);
        }
    }
    onChangeCategory() {
        if (this.f.idAdCategory.value) {
            this.loadingCatParams = true;
            this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].getCategoryParams(this.f.idAdCategory.value), {}).pipe()
