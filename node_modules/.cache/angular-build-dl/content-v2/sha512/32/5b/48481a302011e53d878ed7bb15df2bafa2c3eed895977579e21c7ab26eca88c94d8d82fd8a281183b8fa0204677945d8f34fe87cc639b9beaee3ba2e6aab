class LanguageListComponent {
    constructor(auth, dialog, api, toastr) {
        this.auth = auth;
        this.dialog = dialog;
        this.api = api;
        this.toastr = toastr;
        this.loading = true;
        this.isDeleting = false;
        this.isError = false;
        this.errorMessage = "Sorry, Something went wrong";
        this.selectedLanguage = new src_app_models_language__WEBPACK_IMPORTED_MODULE_1__["Language"]();
        this.tableloading = true;
        this.paginationConfig = _constants__WEBPACK_IMPORTED_MODULE_2__["paginationConfig"];
        this.maxsize = 20;
        auth.checkAuth();
    }
    ngOnInit() {
        this.languages = [];
        this.getLanguages();
    }
    pageChanged(event) {
        this.paginationConfig.currentPage = event;
        this.getLanguages(event);
    }
    getLanguages(page = 1) {
        this.loading = true;
        let data = {
            page: page,
            pageLength: this.maxsize
        };
        this.api.postApiData(_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].languageList, data).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.languages = data.data;
            if (data.meta.pageLength && data.meta.page) {
                this.paginationConfig.totalItems = data.meta.total;
                this.paginationConfig.currentPage = data.meta.page;
                this.paginationConfig.itemsPerPage = data.meta.pageLength;
                this.paginationConfig.maxSize = data.meta.pageLength;
                this.tableloading = false;
            }
            else {
                this.tableloading = true;
            }
        }, error => {
            this.loading = false;
            this.isError = true;
            this.tableloading = false;
        });
    }
    showDeleteModal(language) {
        this.isDeleting = false;
        // let inputElement: HTMLElement = this.deleteModalButton.nativeElement as HTMLElement;
        // inputElement.click();
        const deleteDialog = this.dialog.open(_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_4__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: `Are you sure, you want to delete ${language.languageName} Language?`
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.selectedLanguage = language;
                this.delete();
            }
        });
    }
    delete() {
        if (this.selectedLanguage) {
            this.isDeleting = true;
            this.api.putApiData(_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].locationDelete(this.selectedLanguage.idLanguage)).pipe()
                .subscribe((data) => {
                this.isDeleting = false;
                this.toastr.success('Language deleted successfully', "Deleted");
                let index = this.languages.indexOf(this.selectedLanguage);
                if (index >= -1) {
                    this.languages.splice(index, 1);
                }
                this.selectedLanguage = new src_app_models_language__WEBPACK_IMPORTED_MODULE_1__["Language"]();
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
        else {
            alert('Language not found');
        }
    }
}
