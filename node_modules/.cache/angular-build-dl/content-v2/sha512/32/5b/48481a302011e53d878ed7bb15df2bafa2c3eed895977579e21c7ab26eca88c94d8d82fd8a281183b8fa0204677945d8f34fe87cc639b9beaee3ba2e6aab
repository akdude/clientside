    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("ngIf", ctx_r1.submitted && ctx_r1.f.categoryHoverColor.errors);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵadvance"](8);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("ngForOf", ctx_r1.params().controls);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵadvance"](3);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵtextInterpolate1"]("", ctx_r1.actionType == "Add" ? ctx_r1.isSaving ? "Creating..." : "Create" : ctx_r1.isSaving ? "Updating..." : "Update", " ");
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵadvance"](1);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("ngIf", ctx_r1.isSaving);
} }
class AddCategoryComponent {
    constructor(toastr, _fb, _api, router, _ac) {
        this.toastr = toastr;
        this._fb = _fb;
        this._api = _api;
        this.router = router;
        this._ac = _ac;
        this.actionType = 'Add';
        this.title = 'Add New Category';
        this.submitted = false;
        this.loading = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.idAdCategory = '';
        this.categoryData = '';
        this.parentList = [];
        this.uploadedimage = '';
        this.blockdata = [];
    }
    ngOnInit() {
        this.createCategoryFormInit();
        this.getCategoryList();
        this._ac.params.subscribe(params => {
            this.idAdCategory = params.id;
            if (this.idAdCategory) {
                this.getCategory();
            }
        });
        localStorage.removeItem('image_url');
    }
    //Initializing Category form for validation
    createCategoryFormInit() {
        this.categoryForm = this._fb.group({
            'categoryTitle': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            'parent': [''],
            'categoryIcon': [''],
            'categoryIconMobile': [''],
            'categoryImage': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            'categoryColor': [''],
            'categoryHoverColor': [''],
            'params': this._fb.array([]),
        });
    }
    get f() { return this.categoryForm.controls; }
    convertToSlug(Text) {
        return Text
            .toLowerCase()
            .replace(/[^\w ]+/g, '')
            .replace(/ +/g, '-');
    }
    //Calling Submit function
    hit() {
        this.submitted = true;
        //Checking form validaton
        this.isSaving = true;
        //Creating Category
        if (this.actionType == 'Add') {
            let data = {
                categoryTitle: this.f.categoryTitle.value,
                categoryIcon: this.f.categoryIcon.value,
                categoryIconMobile: this.f.categoryIconMobile.value,
                categoryColor: this.f.categoryColor.value,
                categoryHoverColor: this.f.categoryHoverColor.value,
            };
            if (this.f.parent.value) {
                data.parent = this.f.parent.value;
            }
            if (this.f.params.value) {
                data.params = [];
            }
            data.categoryImage = localStorage.getItem('image_url');
            if (!data.categoryImage) {
                this.isSaving = false;
                this.toastr.warning('Please Upload any image to continue', 'OOPS!');
                return;
            }
            var paramcount = document.getElementsByClassName("textinput");
            var zz = 0;
            for (let z = 0; z < paramcount.length; z++) {
                data.params[zz] = {};
                if (paramcount[z].attributes[2].value !== 'image') {
                    data.params[zz].pageBlock = paramcount[z].attributes[1].value;
                    data.params[zz].paramType = paramcount[z].attributes[2].value;
                    data.params[zz].customParam = this.convertToSlug(paramcount[z]['cells'][0].children[0].value).slice(0, 10);
                    data.params[zz].paramName = paramcount[z]['cells'][0].children[0].value;
                    data.params[zz].paramDesc = paramcount[z]['cells'][1].children[0].value;
                    data.params[zz].paramSlug = this.convertToSlug(paramcount[z]['cells'][1].children[0].value.replace(/(<([^>]+)>)/gi, ""));
                    data.params[zz].isRequired = paramcount[z]['cells'][2].children[0].value;
                    data.params[zz].defaultValue = paramcount[z]['cells'][3].children[0].value;
                    if (paramcount[z]['cells'][5]) {
                        data.params[zz].regex = paramcount[z]['cells'][5].children[0].value;
                    }
                    else {
                        data.params[zz].regex = '';
                    }
                    zz += 1;
                }
                else {
                    for (let aa = 0; aa < 5; aa++) {
                        data.params[zz] = {};
                        data.params[zz].pageBlock = paramcount[z].attributes[1].value;
                        data.params[zz].paramType = paramcount[z].attributes[2].value;
                        data.params[zz].customParam = 'image' + (aa + 1);
                        data.params[zz].paramName = paramcount[z]['cells'][0].children[0].value;
                        data.params[zz].paramDesc = paramcount[z]['cells'][1].children[0].value;
                        data.params[zz].paramSlug = 'image-' + (aa + 1);
                        ;
                        data.params[zz].isRequired = paramcount[z]['cells'][2].children[0].value;
                        data.params[zz].defaultValue = paramcount[z]['cells'][3].children[0].value;
                        data.params[zz].regex = '';
                        zz += 1;
                    }
                }
            }
            var zy = data.params.length;
            var paramcount = document.getElementsByClassName("radioinput");
            for (let y = 0; y < paramcount.length; y++) {
                data.params[zy] = {};
                data.params[zy].pageBlock = paramcount[y].attributes[1].value;
                data.params[zy].paramType = paramcount[y].attributes[2].value;
                data.params[zy].customParam = this.convertToSlug(paramcount[y]['cells'][0].children[0].value).slice(0, 10);
                data.params[zy].paramName = paramcount[y]['cells'][0].children[0].value;
                data.params[zy].paramDesc = paramcount[y]['cells'][1].children[0].value;
                data.params[zy].paramSlug = this.convertToSlug(paramcount[y]['cells'][1].children[0].value.replace(/(<([^>]+)>)/gi, ""));
                data.params[zy].isRequired = paramcount[y]['cells'][2].children[0].value;
                data.params[zy].defaultValue = paramcount[y]['cells'][3].children[0].value;
                zy += 1;
            }
            this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryCreate, data).subscribe((data) => {
                this.isSaving = false;
                this.submitted = false;
                this.toastr.success('Category created successfully', "Created");
                this.router.navigate(['category/list']);
            }, (error) => {
                this.errorMessage = error && error.message ? error.message : 'Sorry, Something went wrong';
                this.isSaving = false;
                this.submitted = false;
            });
        }
        //Editing Category details
        if (this.actionType == 'Edit') {
            let data = {
                categoryTitle: this.f.categoryTitle.value
            };
            if (this.f.parent.value !== "0") {
                data.parent = this.f.parent.value;
            }
            if (this.f.categoryIcon.value) {
                data.categoryIcon = this.f.categoryIcon.value;
            }
            if (this.f.categoryIconMobile.value) {
                data.categoryIconMobile = this.f.categoryIconMobile.value;
            }
            if (this.f.categoryColor.value) {
                data.categoryColor = this.f.categoryColor.value;
            }
            if (this.f.categoryHoverColor.value) {
                data.categoryHoverColor = this.f.categoryHoverColor.value;
            }
            if (this.f.params.value) {
                data.params = [];
            }
            if (localStorage.getItem('image_url')) {
                data.categoryImage = localStorage.getItem('image_url');
            }
            var paramcount = document.getElementsByClassName("textinput");
            var zz = 0;
            for (let z = 0; z < paramcount.length; z++) {
                if ((paramcount[z].attributes[1].value !== 'Contact') && (paramcount[z].attributes[1].value !== 'Location')) {
                    data.params[zz] = {};
                    if (paramcount[z].attributes[2].value !== 'image') {
                        data.params[zz].pageBlock = paramcount[z].attributes[1].value;
                        data.params[zz].paramType = paramcount[z].attributes[2].value;
                        data.params[zz].customParam = this.convertToSlug(paramcount[z]['cells'][0].children[0].value).slice(0, 10);
                        data.params[zz].paramName = paramcount[z]['cells'][0].children[0].value;
                        data.params[zz].paramDesc = paramcount[z]['cells'][1].children[0].value;
                        data.params[zz].paramSlug = this.convertToSlug(paramcount[z]['cells'][1].children[0].value.replace(/(<([^>]+)>)/gi, ""));
                        data.params[zz].isRequired = paramcount[z]['cells'][2].children[0].value;
                        data.params[zz].defaultValue = paramcount[z]['cells'][3].children[0].value;
                        if (paramcount[z]['cells'][5]) {
                            data.params[zz].regex = paramcount[z]['cells'][5].children[0].value;
                        }
                        else {
                            data.params[zz].regex = '';
                        }
                        zz += 1;
                    }
                    else {
                        for (let aa = 0; aa < 5; aa++) {
                            data.params[zz] = {};
                            data.params[zz].pageBlock = paramcount[z].attributes[1].value;
                            data.params[zz].paramType = paramcount[z].attributes[2].value;
                            data.params[zz].customParam = 'image' + (aa + 1);
                            data.params[zz].paramName = paramcount[z]['cells'][0].children[0].value;
                            ;
                            data.params[zz].paramDesc = paramcount[z]['cells'][1].children[0].value;
                            data.params[zz].paramSlug = 'image-' + (aa + 1);
                            ;
                            data.params[zz].isRequired = paramcount[z]['cells'][2].children[0].value;
                            data.params[zz].defaultValue = paramcount[z]['cells'][3].children[0].value;
                            data.params[zz].regex = '';
                            zz += 1;
                        }
                    }
                }
            }
            var zy = data.params.length;
            var paramcount = document.getElementsByClassName("radioinput");
            for (let y = 0; y < paramcount.length; y++) {
                if ((paramcount[y].attributes[1].value !== 'Contact') && (paramcount[y].attributes[1].value !== 'Location')) {
                    data.params[zy] = {};
                    data.params[zy].pageBlock = paramcount[y].attributes[1].value;
                    data.params[zy].paramType = paramcount[y].attributes[2].value;
                    data.params[zy].customParam = this.convertToSlug(paramcount[y]['cells'][0].children[0].value).slice(0, 10);
                    data.params[zy].paramName = paramcount[y]['cells'][0].children[0].value;
                    data.params[zy].paramDesc = paramcount[y]['cells'][1].children[0].value;
                    data.params[zy].paramSlug = this.convertToSlug(paramcount[y]['cells'][1].children[0].value.replace(/(<([^>]+)>)/gi, ""));
                    data.params[zy].isRequired = paramcount[y]['cells'][2].children[0].value;
                    data.params[zy].defaultValue = paramcount[y]['cells'][3].children[0].value;
                    zy += 1;
                }
            }
            this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryUpdate(this.idAdCategory), data).subscribe((data) => {
                this.isSaving = false;
                this.submitted = false;
                this.toastr.success('Category updated successfully', "Updated");
                this.router.navigate(['category/list']);
            }, (error) => {
                this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                this.isSaving = false;
                this.submitted = false;
            });
        }
    }
    onSelectFile(event) {
        if (event.target.files && event.target.files[0]) {
            var reader = new FileReader();
            var id = 'cat_image';
            reader.readAsDataURL(event.target.files[0]);
            reader.onload = (event) => {
                var url = event.target.result;
                localStorage.setItem('image_url', url.toString());
                var elem = '<img src = ' + url + ' class="c-image"></img>';
                var q = document.getElementById('image_html');
                if (q && q.nextElementSibling && q.nextElementSibling.classList.contains('c-image')) {
                    q.nextElementSibling.remove();
                }
                document.getElementById('image_html').insertAdjacentHTML('afterend', elem);
            };
        }
    }
    addcatparams(i) {
        var div = document.getElementById('catparam_' + i);
        var elem = document.getElementById('pageblock_' + i);
        if (elem.value == '') {
            return;
        }
        function capitalizeFirstLetter(string) {
            return string.charAt(0).toUpperCase() + string.slice(1);
        }
        var inputtypes = '<select class="form-control mt-10 ml-10 c-type option-type" id="input_' + i + '">' +
            '                <option>text</option>' +
            '                <option>radio</option>' +
            '                <option>options</option>' +
            '                <option>checkbox</option>' +
            ((elem.value == 'Media') ?
                '                <option>image</option>' : '') +
            '                <option>file</option>' +
            '            </select>';
        div.insertAdjacentHTML('beforeend', '<div>' + '<table class="table page-div" id="pagediv_' + i + '" data-pageblock="' + elem.value + '">' +
            '<th class="limegreen">' + 'Category Parameters of Pageblock : ' + elem.value + '</th>' + '<button type="button" onclick="this.parentElement.remove()" id="custom-close_' + i + '" class="custom-close btn btn-danger">' +
            '    <i class="fa fa-times" aria-hidden="true"> </i>' +
            '</button>' +
            '<table class="final-div" id="final_div_' + i + '">' +
            '<td>' + '<b>' + '' + ' ' + '</b>' + '</td>' +
            '</table>' +
            '       <td>' +
            '            <span class="ml-10">Type</span>' + '<button type="button" onclick="event.stopPropagation();" class="input-div add-final-div btn btn-primary" id="inputdiv_' + i + '">' +
            '    <i class="fa fa-plus" aria-hidden="true"> </i>' +
            '</button>' + inputtypes +
            '        </td>' +
            '</table>' + '</div>');
        var abcElements = document.querySelectorAll('.page-div');
        for (var k = 0; k < abcElements.length; k++) {
            if (abcElements[k].id !== 'pagediv-' + k) {
                abcElements[k].id = 'pagediv-' + k;
            }
            ;
        }
        var abcElements = document.querySelectorAll('.final-div');
        for (var k = 0; k < abcElements.length; k++) {
            if (abcElements[k].id !== 'finaldiv-' + k) {
                abcElements[k].id = 'finaldiv-' + k;
            }
            ;
        }
        var abcElements = document.querySelectorAll('.option-type');
        for (var k = 0; k < abcElements.length; k++) {
            if (abcElements[k].id !== 'input-' + k) {
                abcElements[k].id = 'input-' + k;
            }
            ;
        }
        var abcElements = document.querySelectorAll('.input-div');
        for (var k = 0; k < abcElements.length; k++) {
            if (abcElements[k].id !== 'inputdiv-' + k) {
                abcElements[k].id = 'inputdiv-' + k;
                gen(k);
            }
        }
        function gen(k) {
            var elem2 = document.getElementById('inputdiv-' + k);
            if (elem2) {
                elem2.addEventListener('click', function () {
                    var final_div = document.getElementById('finaldiv-' + k);
                    var input_type = document.getElementById('input-' + k).value;
                    var pageblock_type = document.getElementById('pagediv-' + k).dataset.pageblock;
                    if ((input_type !== 'radio') && (input_type !== 'options')) {
                        var text_default = '<td>' +
                            '    Placeholder' +
                            '    <input type="text" id="default_' + i + '" required="" placeholder="Values seperated with comma" class="form-control">' +
                            '</td>';
                        var check_default = '<td class="pt-0">' +
                            '    Selected' +
                            '<select required="" id="default_' + i + '" class="form-control w-70">' +
                            '        <option value="true">True</option>' +
                            '        <option value="false">False</option>' +
                            '    </select>' +
                            '</td>';
                        if (input_type == 'checkbox') {
                            var default_div = check_default;
                        }
                        else {
                            var default_div = text_default;
                        }
                        if (input_type == 'image') {
                            var otid = 'pageblock_image';
                        }
                        else {
                            var otid = "";
                        }
                        if (input_type == 'text') {
                            var regex = '<td class="pt-0">Regex Type<select required="" id="regex_' + i + '" class="form-control w-96 regextype">' +
                                '<option>Select</option>' +
                                '        <option value="\\D*([2-9]\\d{2})(\\D*)([2-9]\\d{2})(\\D*)(\\d{4})\\D*">Phone</option>' +
                                '        <option value="^([a-zA-Z0-9_\\-\\.]+)@([a-zA-Z0-9_\\-\\.]+).([a-zA-Z]{2,5})$">Email</option>' +
                                '          <option value="(http(s)?:\\\\)?([\\w-]+\\.)+[\\w-]+[.com|.in|.org]+(\\[\\?%&=]*)?">Url</option>' +
                                '<option value="">Others</option>' +
                                '    </select></td>' +
                                '<td>' +
                                '    Regex Value' +
                                '    <input type="text" required="" id="regex_value_' + i + '" class="form-control mb-10 w-110 regexvalue">' +
                                '</td>';
                        }
                        else {
                            var regex = "";
                        }
                        var exist = document.getElementsByClassName('pageblock_image');
                        if (otid && exist.length > 0) {
                            return;
                        }
                        final_div.insertAdjacentHTML('beforeend', '<div><th class="ml-10"><u>' + (capitalizeFirstLetter(input_type)) + '</u></th>' +
                            '<tr class="textinput ' + otid + '" data-pageBlock = "' + pageblock_type + '" data-paramtype="' + input_type + '">' +
                            '<td>' +
                            '    Name' +
                            '    <input type="text"  id="name_' + i + '" required="" class="form-control">' +
                            '</td>' +
                            '<td>' +
                            '    Description' +
                            '    <input type="text" id="desc_' + i + '" required="" class="form-control">' +
                            '</td>' +
                            '<td class="pt-0">' +
                            '    Required ' +
                            '    <select required="" id="required_' + i + '" class="form-control w-70">' +
                            '        <option value="false">No</option>' +
                            '        <option value="true">Yes</option>' +
                            '    </select>' +
                            '</td>' +
                            default_div +
                            regex +
                            ((otid) ? '<td><span style="color:red;">* <i>5 images will be rendered</i></span></td>' : ' ') +
                            '<td class="relative bot-50">' +
                            '<button type="button" onclick="this.parentElement.parentElement.parentElement.previousElementSibling.remove();this.parentElement.parentElement.parentElement.remove();this.remove();" id="input-close_' + i + '" class="btn btn-danger relative">' +
                            '    <i class="fa fa-times" aria-hidden="true"> </i>' +
                            '</button>' +
                            '</td>' +
                            '</tr>' +
                            '</div>');
                        var aabcElements = document.getElementsByClassName('regextype');
                        if (aabcElements) {
                            for (var kk = 0; kk < aabcElements.length; kk++) {
                                if (aabcElements[kk].id !== 'regex_' + kk) {
                                    aabcElements[kk].id = 'regex_' + kk;
                                }
                                regexchange(kk);
                            }
                        }
                        var aaabcElements = document.getElementsByClassName('regexvalue');
                        if (aaabcElements) {
                            for (var kj = 0; kj < aaabcElements.length; kj++) {
                                if (aaabcElements[kj].id !== 'regex_value_' + kj) {
                                    aaabcElements[kj].id = 'regex_value_' + kj;
                                }
                            }
                        }
                        function regexchange(i) {
                            var regsel = document.getElementById('regex_' + i);
                            if (regsel) {
                                regsel.addEventListener('click', function () {
                                    var value = regsel.value;
                                    if (value !== 'Select') {
                                        document.getElementById('regex_value_' + i).value = value;
                                    }
                                    else {
                                        document.getElementById('regex_value_' + i).value = '';
                                    }
                                });
                            }
                        }
                    } //non radio inputs
                    if ((input_type == 'radio') || (input_type == 'options')) {
                        final_div.insertAdjacentHTML('beforeend', '<th class="ml-10"><u>' + (capitalizeFirstLetter(input_type)) + '</u></th>' +
                            '<tr class="h-40 radiodiv" data-pageblock = "' + pageblock_type + '" data-radiotype="' + input_type + '" id="radiosec_' + i + '">' +
                            '<td class="relative c-550">' +
                            '<button type="button" onclick="event.stopPropagation();" id="input-radio-add_' + i + '" class="btn btn-primary relative cc-1 radio-add-button">' +
                            '    <i class="fa fa-plus" aria-hidden="true"> </i>' +
                            '</button>' +
                            '<button type="button" onclick="this.parentElement.parentElement.parentElement.remove();" id="input-radio-close_' + i + '" class="btn btn-danger relative ml-20 cc-2 ">' +
                            '    <i class="fa fa-times" aria-hidden="true"> </i>' +
                            '</button>' +
                            '</td>' +
                            '</tr>');
                        var abcElements = document.querySelectorAll('.radiodiv');
                        for (var z = 0; z < abcElements.length; z++) {
                            if (abcElements[z].id !== 'radiodiv-' + z) {
                                abcElements[z].id = 'radiodiv-' + z;
                            }
                        }
                        var abcElements = document.querySelectorAll('.radio-add-button');
                        for (var zx = 0; zx < abcElements.length; zx++) {
                            if (abcElements[zx].id !== 'radiobtn-' + zx) {
                                abcElements[zx].id = 'radiobtn-' + zx;
                                next(zx);
                                if (window.location.pathname == '/category/add') {
                                    document.getElementById('radiobtn-' + zx).click();
                                }
                            }
                        }
                    }
                    function next(zx) {
                        var inputadd = document.getElementById('radiobtn-' + zx);
                        if (inputadd) {
                            inputadd.addEventListener('click', function () {
                                optiondata(zx);
                            });
                        }
                    }
                });
            }
        }
        function optiondata(zx) {
            var final_div = document.getElementById('radiodiv-' + zx);
            var pageBlock = final_div.dataset.pageblock;
            var paramType = final_div.dataset.radiotype;
            final_div.insertAdjacentHTML('afterend', '<tr class="radioinput" data-pageblock = "' + pageBlock + '" data-paramtype="' + paramType + '">' + '<td>' +
                '    Name' +
                '    <input type="text" required="" class="form-control radioname-' + zx + '">' +
                '</td>' +
                '<td>' +
                '    Description' +
                '    <input type="text" required="" class="form-control">' +
                '</td>' +
                '<td class="pt-0">' +
                '    Required ' +
                '    <select required="" class="form-control w-70">' +
                '        <option>Yes</option>' +
                '        <option>No</option>' +
                '    </select>' +
                '</td>' +
                '<td>' +
                '    Selected' +
                '    <select required="" class="form-control w-70 mb-10">' +
                '        <option value="true">True</option>' +
                '        <option value="false">False</option>' +
                '    </select>' +
                '</td>' +
                '<td>' +
                '<button type="button" onclick="this.parentElement.parentElement.children[0].remove();this.parentElement.parentElement.children[0].remove();this.parentElement.parentElement.children[0].remove();this.parentElement.parentElement.children[0].remove();this.parentElement.parentElement.children[0].remove();" id="curent-radio-close_' + i + '" class="btn btn-danger relative mt-10">' +
                '    <i class="fa fa-times" aria-hidden="true"> </i>' +
                '</button>' +
                '</td>' + '</tr>');
            var elements = document.querySelectorAll('.radioname-' + zx);
            var cnumber = zx;
            for (var i = 0; i < elements.length; i++) {
                elements[i].addEventListener('focusout', fillfocus, false);
            }
            function fillfocus() {
                var fillvalue = this.value;
                var inputs = document.querySelectorAll('.radioname-' + cnumber);
                for (let i = 0; i < inputs.length; i++) {
                    if (fillvalue) {
                        inputs[i].value = fillvalue;
                    }
                }
            }
        }
    }
    //Getting Category data using (idAdCategory)
    getCategory() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryGet(this.idAdCategory)).subscribe((data) => {
            this.categoryData = data.data;
            if (this.categoryData) {
                this.setcategoryData(this.categoryData);
            }
        });
    }
    //Setting category data in edit form
    setcategoryData(data) {
        let parameters = data.params;
        this.blockdata = data.params;
        let paramsFields = this.categoryForm.get('params');
        paramsFields.removeAt(0);
        this.categoryForm.patchValue({
            'categoryTitle': data.categoryTitle,
            'parent': data.parent,
            'categoryIcon': data.categoryIcon,
            'categoryIconMobile': data.categoryIconMobile,
            'categoryColor': data.categoryColor,
            'categoryImage': data.categoryImage,
            'categoryHoverColor': data.categoryHoverColor
        });
        parameters.forEach(ele => {
            paramsFields.push(this.newParams(ele.paramName, ele.paramDesc, ele.paramType, ele.paramSlug, ele.pageBlock, ele.isRequired, ele.customParam, ele.defaultValue));
        });
        this.loading = false;
        setTimeout(function () {
            var radio_count = 0;
            var radio_ids = [];
            var radio_tdata = [];
            var radio_rdata = [];
            var radio_idata = [];
            var radio_data = [];
            var blocks = data.params;
            var url = data.categoryImage.split(' ').join('%20');
            var id = 'cat_image';
            var elem = '<img src = "' + url + '" class="c-image" onerror=this.src="https://code.recuweb.com/c/u/3a09f4cf991c32bd735fa06db67889e5/2018/08/wordpress-photo-gallery-plugins1.png"></img>';
            document.getElementById('image_html').insertAdjacentHTML('afterend', elem);
            for (let i = 0; i < blocks.length; i++) {
                var q = document.getElementById("pageblock_" + i);
                q.value = blocks[i]['block'];
                var r = document.getElementById("addblock_" + i);
                r.click();
                for (let j = 0; j < blocks[i]['params'].length; j++) {
                    var l = document.getElementById("input-" + i);
                    if (!l) {
                        return;
                    }
                    l.value = blocks[i]['params'][j]['paramType'];
                    var m = document.getElementById("inputdiv-" + i);
                    m.click();
                    var abcElements = document.querySelectorAll('.textinput');
                    for (var k = 0; k < abcElements.length; k++) {
                        if (abcElements[k].id !== 'text-' + k) {
                            abcElements[k].id = 'text-' + k;
                            var tr = document.getElementById('text-' + k);
                            tr["cells"][0].firstElementChild.value = blocks[i]['params'][j]['title'];
                            tr["cells"][1].firstElementChild.value = blocks[i]['params'][j]['values'][0]['lable'];
                            tr["cells"][2].firstElementChild.value = blocks[i]['params'][j]['isRequired'].toString();
                            tr["cells"][3].firstElementChild.value = (blocks[i]['params'][j]['values'][0]['placeholder']) ? (blocks[i]['params'][j]['values'][0]['placeholder']) : null;
                            if (tr["cells"][5]) {
                                tr["cells"][5].firstElementChild.value = (blocks[i]['params'][j]['values'][0]['regex']) ? (blocks[i]['params'][j]['values'][0]['regex']) : null;
                            }
                        }
                    }
                    if (blocks[i]['params'][j]['paramType'] == 'options' || blocks[i]['params'][j]['paramType'] == 'radio') {
                        var lengthclick = blocks[i]['params'][j]['values'].length;
                        for (let index = 0; index < lengthclick; index++) {
                            var rbtn = document.getElementById("radiobtn-" + radio_count);
                            rbtn.click();
                            var removefirstradio = document.querySelectorAll('.radioinput');
                            for (let de = 0; de < removefirstradio.length; de++) {
                                removefirstradio[de].remove;
                            }
                            var xyzElements = document.querySelectorAll('.radioinput');
                            for (var n = 0; n < xyzElements.length; n++) {
                                if (xyzElements[n].id !== 'radio-' + n) {
                                    xyzElements[n].id = 'radio-' + n;
                                    var currentradio = n;
                                }
                            }
                            radio_ids.push(currentradio);
                            radio_data.push(blocks[i]['params'][j]['values'][index]);
                            radio_tdata.push({ 'title': blocks[i]['params'][j]['title'] });
                            radio_rdata.push({ 'isRequired': blocks[i]['params'][j]['isRequired'] });
                            radio_idata.push({ 'inputName': blocks[i]['params'][j]['inputName'] });
                        }
                        radio_count += 1;
                    }
                }
            }
            for (let i = 0; i < radio_ids.length; i++) {
                var trp = document.getElementById('radio-' + radio_ids[i]);
                trp["cells"][0].firstElementChild.value = radio_tdata[i]['title'];
                trp["cells"][1].firstElementChild.value = radio_data[i]['lable'];
                trp["cells"][2].firstElementChild.value = (radio_rdata[i]['isRequired']) ? "Yes" : "No";
                trp["cells"][3].firstElementChild.value = (radio_data[i]['selected']) ? (radio_data[i]['selected']) : (radio_data[i]['checked']);
            }
            var mainpages = document.getElementsByClassName('mainpageblock');
            for (let m = 0; m < mainpages.length; m++) {
                if (m !== 0) {
                    mainpages[m].innerHTML = '';
                }
            }
        }, 2000);
        setTimeout(function () {
            var blocks = data.params;
            for (let i = 0; i < blocks.length; i++) {
                if ((blocks[i]['block'] == 'Location') || (blocks[i]['block'] == 'Contact')) {
                    document.getElementById('input-' + i).previousElementSibling.previousElementSibling.remove();
                    document.getElementById('input-' + i).classList.add('hide');
                    document.getElementById('inputdiv-' + i).classList.add('hide');
                    document.getElementById('custom-close_' + i).disabled = true;
                    let disabled = document.getElementById("finaldiv-" + i).querySelectorAll("button,select,input");
                    for (let i = 0; i < disabled.length; i++) {
                        const element = disabled[i];
                        element.disabled = true;
                    }
                }
            }
            let pageBlock = document.getElementById('pageblock_0');
            pageBlock.remove(4);
            pageBlock.remove(3);
        }, 2000);
    }
    //Getting category list
    getCategoryList() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].categoryList).subscribe((data) => {
            this.loading = false;
            this.parentList = data.data;
        }, error => {
            this.loading = false;
        });
    }
    //Getting parameter values in FormArray
    params() {
        return this.categoryForm.get("params");
    }
    newParams(id = '', name = '', desc = '', type = '', block = '', isrequired = '', customparam = '', value = '') {
        return this._fb.group({
            paramName: [name],
            paramDesc: [desc],
            paramType: [type],
            pageBlock: [block],
            isRequired: [isrequired],
            customParam: [customparam],
            defaultValue: [value]
        });
    }
    addParams() {
        var exist = document.getElementById('pageblock_0');
        if (exist) {
            return;
        }
        this.params().push(this.newParams());
        setTimeout(function () {
            let pageBlock = document.getElementById('pageblock_0');
            pageBlock.remove(4);
            pageBlock.remove(3);
        }, 1000);
    }
    removeParams(i) {
        this.params().removeAt(i);
    }
    //Cancel Category
    cancelcategoryCreate() {
        this.router.navigate(['category/list']);
    }
}
