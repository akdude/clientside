class TopNavbarComponent {
    constructor(api, route, actRoute, toastr) {
        this.api = api;
        this.route = route;
        this.actRoute = actRoute;
        this.toastr = toastr;
        this.username = localStorage.getItem('Username');
    }
    ngOnInit() {
    }
    stopPro(event) {
        event.stopPropagation();
        return false;
    }
    logout() {
        var currentUrl = window.location.href;
        document.querySelector('.top-navbar-fixed').innerHTML = '<center>Logging Out ..</center>';
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_1__["apiUrls"].userLogout()).subscribe((data) => {
            window.location.href = '/accounts/login';
            localStorage.clear();
        }, error => {
            setTimeout(function () { window.location.href = currentUrl; }, 2000);
        });
    }
}
