class SettingsComponent {
    constructor(_ac, _fb, _api, _router, toastr, dialog, auth) {
        this._ac = _ac;
        this._fb = _fb;
        this._api = _api;
        this._router = _router;
        this.toastr = toastr;
        this.dialog = dialog;
        this.auth = auth;
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_3__["paginationConfig"];
        this.isDeleting = false;
        this.tableloading = true;
        this.maxsize = 20;
        this.loading = false;
        this.settings = '';
        this.errorMessage = "";
        this.isSaving = false;
        this.submitted = false;
        this.isError = false;
        this.logo = '';
        this.selectedSetting = new src_app_models_setting__WEBPACK_IMPORTED_MODULE_4__["Setting"]();
        this.idSetting = '';
        auth.checkAuth();
    }
    ngOnInit() {
        this.settings = [];
        this.getSettings();
        this.createSettingsFormInit();
    }
    //Initializing Userform for validation
    createSettingsFormInit() {
        this.settingsForm = this._fb.group({
            'key': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            'value': ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
        });
    }
    get f() { return this.settingsForm.controls; }
    //Getting User data using (idUser)
    getSettings() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].getSettings).subscribe((data) => {
            this.settings = data.data;
            if (this.settings) {
                this.loading = false;
                if (data.meta.pageLength && data.meta.page) {
                    this.paginationConfig.totalItems = data.meta.total;
                    this.paginationConfig.currentPage = data.meta.page;
                    this.paginationConfig.itemsPerPage = data.meta.pageLength;
                    this.paginationConfig.maxSize = data.meta.pageLength;
                    this.tableloading = false;
                }
                else {
                    this.tableloading = true;
                }
                // this.setsettingsData(this.settings);
            }
        });
    }
    pageChanged(event) {
        this.paginationConfig.currentPage = event;
        // this.getTranslations(event);
    }
    openEdit(setting) {
        this.idSetting = setting.idSettings;
        const deleteDialog = this.dialog.open(src_app_common_dialog_setting_dialog_setting_dialog_component__WEBPACK_IMPORTED_MODULE_6__["SettingDialogComponent"], {
            width: '500px',
            height: '350px',
            data: {
                editSetting: setting
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.onSubmitEdit(data.data);
            }
        });
    }
    onSubmitEdit(response) {
        this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].settingUpdate(this.idSetting), response).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.toastr.success('Settings Updated successfully', "Updated");
            this.getSettings();
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
        });
    }
    //Setting User data in edit form
    // setsettingsData(data) {
    //   this.settingsForm.patchValue({
    //     'key': data.key,
    //     'value': data.value
    //   });
    //   this.idSetting = data.idSettings
    // }
    onSubmit() {
        // this.f.logo.value ? (data.logo = this.f.logo.value) : '';console.log(data)
        if (this.settingsForm.invalid) {
            return;
        }
        this.isError = false;
        this.isSaving = true;
        this.submitted = true;
        let data = {
            key: this.f.key.value,
            value: this.f.value.value,
        };
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].createSetting, data).subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.toastr.success('Settings Created successfully', "Created");
            this.settingsForm.reset();
            this.getSettings();
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
        });
    }
    //Function for uploading image
    // handleInputChange(e) {
    //   var file = e.dataTransfer ? e.dataTransfer.files[0] : e.target.files[0];
    //   console.log(file);
    //   var pattern = /image-*/;
    //   var reader = new FileReader();
    //   if (!file.type.match(pattern)) {
    //     alert('invalid format');
    //     return;
    //   }
    //   reader.onload = this._handleReaderLoaded.bind(this);
    //   reader.readAsDataURL(file);
    // }
    // _handleReaderLoaded(e) {
    //   let reader = e.target;
    //   this.logo = reader.result;
    //   console.log(this.logo);
    //   this.settingsForm.patchValue({ logo: this.logo })
    //   console.log(this.logo)
    // }
    showDeleteModal(setting) {
        this.isDeleting = false;
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_5__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure, you want to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.selectedSetting = setting;
                this.delete();
            }
        });
    }
    delete() {
        if (this.selectedSetting) {
            this.isDeleting = true;
            this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].settingDelete(this.selectedSetting.idSettings)).pipe()
                .subscribe((data) => {
                this.isDeleting = false;
                this.toastr.success('Translation deleted successfully', "Deleted");
                let index = this.settings.indexOf(this.selectedSetting);
                if (index >= -1) {
                    this.settings.splice(index, 1);
                }
                this.selectedSetting = new src_app_models_setting__WEBPACK_IMPORTED_MODULE_4__["Setting"]();
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
        else {
            alert('Language not found');
        }
    }
}
