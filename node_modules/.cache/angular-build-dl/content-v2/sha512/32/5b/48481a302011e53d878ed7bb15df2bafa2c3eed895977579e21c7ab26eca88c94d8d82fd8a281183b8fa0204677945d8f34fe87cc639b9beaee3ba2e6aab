class TranslationUpsertComponent {
    constructor(api, formBuilder, route, _ar, toastr) {
        this.api = api;
        this.formBuilder = formBuilder;
        this.route = route;
        this._ar = _ar;
        this.toastr = toastr;
        this.submitted = false;
        this.isError = false;
        this.errorMessage = "";
        this.loading = false;
        this.isSaving = false;
        this.loadingTranslations = false;
    }
    ngOnInit() {
        this.getLanguages();
        this._ar.data.subscribe(data => {
            this.title = data.type;
        });
        this.formGroupInit();
        this._ar.params.subscribe(params => {
            this.id = params.id;
            if (this.id) {
                this.getTranslation();
            }
        });
    }
    formGroupInit() {
        this.translationForm = this.formBuilder.group({
            idLanguage: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            slug: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
            text: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.translationForm.controls; }
    getTranslation() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].translationGet(this.id)).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.translation = data.data;
            this.translationForm = this.formBuilder.group({
                idLanguage: [this.translation.idLanguage, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
                slug: [this.translation.slug, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
                text: [this.translation.text, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
            });
        }, error => {
            this.loading = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong';
        });
    }
    getLanguages() {
        this.loading = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].languageList, {}).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.languages = data.data;
        }, error => {
            this.loading = false;
            this.isError = true;
        });
    }
    onSubmit() {
        this.submitted = true;
        if (this.translationForm.invalid) {
            return;
        }
        this.isError = false;
        this.isSaving = true;
        if (!this.id) {
            this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].translationCreate, this.translationForm.getRawValue()).pipe()
                .subscribe((data) => {
                this.isSaving = false;
                this.submitted = false;
                this.toastr.success('Translation created successfully', 'Done');
                this.route.navigate(['/translation/list']);
            }, (error) => {
                this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                this.isSaving = false;
                this.submitted = false;
                this.isError = true;
            });
        }
        else {
            let data = this.translationForm.getRawValue();
            data['idLanguage'] = this.translation.idLanguage;
            this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].translationUpdate(this.id), data).pipe()
                .subscribe((data) => {
                this.isSaving = false;
                this.submitted = false;
                this.toastr.success('Translation updated successfully', 'Done');
                this.route.navigate(['/translation/list']);
            }, (error) => {
                this.isSaving = false;
                this.submitted = false;
                this.isError = true;
                this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong.';
            });
        }
    }
}
