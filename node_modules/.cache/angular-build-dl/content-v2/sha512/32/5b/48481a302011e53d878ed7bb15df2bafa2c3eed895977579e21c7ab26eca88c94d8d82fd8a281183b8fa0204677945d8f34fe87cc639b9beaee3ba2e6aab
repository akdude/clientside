class BannerAddComponent {
    constructor(toastr, _fb, router, _api, _ac) {
        this.toastr = toastr;
        this._fb = _fb;
        this.router = router;
        this._api = _api;
        this._ac = _ac;
        this.Editor = _ckeditor_ckeditor5_build_classic__WEBPACK_IMPORTED_MODULE_3__;
        this.actionType = 'Add';
        this.title = 'Create New Banner';
        this.submitted = false;
        this.loading = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.idBanner = '';
        this.bannerData = '';
    }
    ngOnInit() {
        this.createBannerFormInit();
        this._ac.params.subscribe(params => {
            this.idBanner = params.id;
            if (this.idBanner) {
                this.getBanner();
            }
        });
    }
    //Initializing Bannerform for validation
    createBannerFormInit() {
        this.bannerForm = this._fb.group({
            'bannerName': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'text': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'location': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'pageCode': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]]
        });
    }
    get f() { return this.bannerForm.controls; }
    //Calling Submit function
    onSubmit() {
        this.submitted = true;
        //Checking form validaton
        if (this.bannerForm.valid) {
            this.isSaving = true;
            //Creating Banner
            if (this.actionType == 'Add') {
                let data = {
                    bannerName: this.f.bannerName.value,
                    text: this.f.text.value,
                    location: this.f.location.value,
                    pageCode: this.f.pageCode.value
                };
                this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].bannerCreate, data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('Banner created successfully');
                    this.router.navigate(['banner/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, Something went wrong';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
            //Editing Banner details
            if (this.actionType == 'Edit') {
                let data = {
                    bannerName: this.f.bannerName.value,
                    text: this.f.text.value,
                    location: this.f.location.value,
                    pageCode: this.f.pageCode.value
                };
                this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].bannerUpdate(this.idBanner), data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('Banner  updated successfully');
                    this.router.navigate(['banner/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
        }
        else {
            console.log('error');
            return;
        }
    }
    //Getting Banner data using (idBanner)
    getBanner() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].bannerGet(this.idBanner)).subscribe((data) => {
            this.bannerData = data.data;
            if (this.bannerData) {
                this.loading = false;
                this.setbannerData(this.bannerData);
            }
        });
    }
    //Setting Banner data in edit form
    setbannerData(data) {
        this.bannerForm.patchValue({
            'bannerName': data.bannerName,
            'text': data.text,
            'location': data.location,
            'pageCode': data.pageCode
        });
    }
    //Cancel Banner create
    cancelbannercreate() {
        this.router.navigate(['banner/list']);
    }
