class AddUserAclComponent {
    constructor(toastr, _fb, _api, router, _ac) {
        this.toastr = toastr;
        this._fb = _fb;
        this._api = _api;
        this.router = router;
        this._ac = _ac;
        this.actionType = 'Add';
        this.title = 'Add ACL';
        this.submitted = false;
        this.loading = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.idUserAcl = '';
        this.UserAclData = '';
    }
    ngOnInit() {
        this.createUserACLFormInit();
        this._ac.params.subscribe(params => {
            this.idUserAcl = params.id;
            if (this.idUserAcl) {
                this.getUserAcl();
            }
        });
    }
    //Initializing Useracl form for validation
    createUserACLFormInit() {
        this.userAclForm = this._fb.group({
            'namespace': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'route': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]],
            'path': ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]]
        });
    }
    get f() { return this.userAclForm.controls; }
    //Calling Submit function
    onSubmit() {
        this.submitted = true;
        //Checking form validaton
        if (this.userAclForm.valid) {
            this.isSaving = true;
            //Creating Useracl
            if (this.actionType == 'Add') {
                let data = {
                    namespace: this.f.namespace.value,
                    path: this.f.path.value,
                    route: this.f.route.value
                };
                this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userAclCreate, data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('UserACL created successfully', "Created");
                    this.router.navigate(['user-acl/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
            //Editing Useracl details
            if (this.actionType == 'Edit') {
                let data = {
                    namespace: this.f.namespace.value,
                    path: this.f.path.value,
                    route: this.f.route.value
                };
                this._api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userAclUpdate(this.idUserAcl), data).subscribe((data) => {
                    this.isSaving = false;
                    this.submitted = false;
                    this.toastr.success('UserACL updated successfully', "Updated");
                    this.router.navigate(['user-acl/list']);
                }, (error) => {
                    this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
                    this.isSaving = false;
                    this.submitted = false;
                });
            }
        }
        else {
            return;
        }
    }
    //Getting Usertypelist
    getUsertypeList() {
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userTypeList).subscribe((data) => {
            console.log(data.data);
            let resSucc = data.data;
            if (resSucc != '')
                this.UsertypeList = resSucc;
        }, error => {
        }, () => {
        });
    }
    //Getting Useracl data using (idUserAcl)
    getUserAcl() {
        this.loading = true;
        this._api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_2__["apiUrls"].userAclDetail(this.idUserAcl)).subscribe((data) => {
            console.log(data);
            this.UserAclData = data.data;
            if (this.UserAclData) {
                this.loading = false;
                this.setUserAclData(this.UserAclData);
            }
        });
    }
    //Setting Useracl data in edit form
    setUserAclData(data) {
        this.userAclForm.patchValue({
            'namespace': data.namespace,
            'route': data.route,
            'path': data.path,
            'rule': data.rule
        });
    }
    //Cancel UserAcl
    cancelUserAclcreate() {
        this.router.navigate(['user-acl/list']);
    }
}
