class MessagesComponent {
    constructor(pusherService, api) {
        this.pusherService = pusherService;
        this.api = api;
        this.messages = [];
    }
    ngOnInit() {
        this.pusherService.messagesChannel.bind('sent-message', (data) => {
            this.messages.push(data);
            console.log(data);
        });
        //fetch messages
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_1__["apiUrls"].fetchMessage('0a1ee83d-5dd3-49d4-b40e-d43daf1b0a77')).pipe()
            .subscribe((data) => {
            var messages = data.data;
            console.log(messages);
            for (let i = 0; i < messages.length; i++) {
                const element = messages[i];
                this.messages.push(element);
            }
            // this.messages.push();
        }, error => {
        });
    }
    sendMessage(user, text) {
        const message = {
            "name": "dravid",
            "message": "hello"
        };
        var messagevalue = document.getElementById('message_val').value;
        if (!messagevalue) {
            alert('Cant send empty message');
        }
        let json = {
            idApplier: "0a1ee83d-5dd3-49d4-b40e-d43daf1b0a77",
            message: messagevalue
        };
        // this.pusherService.messagesChannel.trigger('sent-message', message);
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_1__["apiUrls"].sendMessage, json).pipe()
            .subscribe((data) => {
            document.getElementById('message_val').value = '';
        }, error => {
            document.getElementById('message_val').value = '';
        });
        // this.messages.push(message);
    }
}
