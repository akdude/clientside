class AdvertisementBadgeComponent {
    constructor(api, formBuilder, route, actRoute, toastr) {
        this.api = api;
        this.formBuilder = formBuilder;
        this.route = route;
        this.actRoute = actRoute;
        this.toastr = toastr;
        this.submitted = false;
        this.isError = false;
        this.errorMessage = "";
        this.isSaving = false;
        this.loadingBadges = false;
        this.loading = true;
        this.idAdvertisement = null;
        this.categoryParams = [];
        this.categoryParamOptions = [];
        this.selectedParamOptions = [];
        this.advertisement = new src_app_models_advertisement__WEBPACK_IMPORTED_MODULE_2__["Advertisement"]();
        this.adminUser = new src_app_models_admin_user__WEBPACK_IMPORTED_MODULE_4__["AdminUser"]();
    }
    ngOnInit() {
        this.getBadges();
        this.setForm();
        this.actRoute.paramMap.subscribe(params => {
            this.idAdvertisement = params.get('id');
        });
    }
    get f() { return this.advertisementForm.controls; }
    getBadges() {
        this.loadingBadges = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].badgeList).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.loadingBadges = false;
            this.badges = data.data;
        }, (error) => {
            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';
            this.loadingBadges = false;
            this.isError = true;
        });
    }
    setForm() {
        this.advertisementForm = this.formBuilder.group({
            idBadge: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
        });
    }
    hit(badgeid) {
        this.submitted = true;
        this.isError = false;
        this.isSaving = true;
        let data = this.advertisementForm.getRawValue();
        let lastbuttons = document.getElementsByClassName('c-green');
        for (let i = 0; i < lastbuttons.length; i++) {
            lastbuttons[i].classList.add('disabled');
        }
        data.idAdvertisement = this.idAdvertisement;
        data.idBadge = badgeid;
        document.getElementById('action_' + badgeid).classList.remove('hide');
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].advertisementBadgeLink, data).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            let lastbuttons = document.getElementsByClassName('c-green');
            document.getElementById('action_' + badgeid).classList.add('hide');
            for (let i = 0; i < lastbuttons.length; i++) {
                lastbuttons[i].classList.remove('disabled');
            }
            this.toastr.success('Badge added successfully', 'Done');
            this.route.navigate(['/advertisements/list']);
        }, (error) => {
            this.errorMessage = 'Sorry, something went wrong.';
            let lastbuttons = document.getElementsByClassName('c-green');
            document.getElementById('action_' + badgeid).classList.add('hide');
            for (let i = 0; i < lastbuttons.length; i++) {
                lastbuttons[i].classList.remove('disabled');
            }
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
        });
    }
}
