class TranslationListComponent {
    constructor(auth, dialog, api, toastr, fb) {
        this.auth = auth;
        this.dialog = dialog;
        this.api = api;
        this.toastr = toastr;
        this.fb = fb;
        this.loading = true;
        this.isDeleting = false;
        this.isError = false;
        this.errorMessage = "Sorry, Something went wrong";
        this.selectedTranslation = new src_app_models_translation__WEBPACK_IMPORTED_MODULE_2__["Translation"]();
        this.tableloading = true;
        this.paginationConfig = _constants__WEBPACK_IMPORTED_MODULE_1__["paginationConfig"];
        this.maxsize = 20;
        this.selectedIdLanguage = '';
        this.searchedValue = '';
        auth.checkAuth();
    }
    ngOnInit() {
        this.translations = [];
        this.getLanguages();
        this.getTranslations();
        // this.formgroupInit();
    }
    // formgroupInit() {
    //   this.formGroup = this.fb.group({
    //     'selectValue': [''],
    //     'searchValue': ['']
    //   })
    // }
    pageChanged(event) {
        this.paginationConfig.currentPage = event;
        this.getTranslations(event);
    }
    getTranslations(page = 1) {
        this.loading = true;
        let data = {
            page: page,
            pageLength: this.maxsize
        };
        if (this.selectedIdLanguage)
            data['idLanguage'] = this.selectedIdLanguage;
        if (this.searchedValue)
            data['searchKeyword'] = this.searchedValue;
        this.api.postApiData(_api_urls__WEBPACK_IMPORTED_MODULE_4__["apiUrls"].translationList, data).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.translations = data.data;
            if (data.meta.pageLength && data.meta.page) {
                this.paginationConfig.totalItems = data.meta.total;
                this.paginationConfig.currentPage = data.meta.page;
                this.paginationConfig.itemsPerPage = data.meta.pageLength;
                this.paginationConfig.maxSize = data.meta.pageLength;
                this.tableloading = false;
            }
            else {
                this.tableloading = true;
            }
        }, error => {
            this.loading = false;
            this.isError = true;
            this.tableloading = false;
        });
    }
    getLanguages() {
        this.api.postApiData(_api_urls__WEBPACK_IMPORTED_MODULE_4__["apiUrls"].languageList, {}).pipe()
            .subscribe((data) => {
            this.languages = data.data;
        }, error => {
            console.log(error);
        });
    }
    selectedLanguage(res) {
        this.selectedIdLanguage = res.idLanguage;
        this.getTranslations();
    }
    showDeleteModal(translation) {
        this.isDeleting = false;
        // let inputElement: HTMLElement = this.deleteModalButton.nativeElement as HTMLElement;
        // inputElement.click();
        const deleteDialog = this.dialog.open(_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_3__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure, you want to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.selectedTranslation = translation;
                this.delete();
            }
        });
    }
    delete() {
        if (this.selectedTranslation) {
            this.isDeleting = true;
            this.api.putApiData(_api_urls__WEBPACK_IMPORTED_MODULE_4__["apiUrls"].locationDelete(this.selectedTranslation.idTranslation)).pipe()
                .subscribe((data) => {
                this.isDeleting = false;
                // let inputElement: HTMLElement = this.cancelModalButton.nativeElement as HTMLElement;
                // inputElement.click();
                this.toastr.success('Translation deleted successfully', "Deleted");
                let index = this.translations.indexOf(this.selectedTranslation);
                if (index >= -1) {
                    this.translations.splice(index, 1);
                }
                this.selectedTranslation = new src_app_models_translation__WEBPACK_IMPORTED_MODULE_2__["Translation"]();
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
        else {
            alert('Language not found');
        }
    }
    searchedSlug(ev) {
        this.searchedValue = ev.target.value;
        this.getTranslations();
    }
}
