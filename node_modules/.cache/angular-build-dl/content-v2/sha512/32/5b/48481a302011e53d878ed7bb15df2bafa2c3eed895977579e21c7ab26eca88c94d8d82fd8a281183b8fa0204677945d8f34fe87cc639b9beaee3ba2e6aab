class EditUserTypeComponent {
    constructor(api, url, formBuilder, route, actRoute, toastr) {
        this.api = api;
        this.url = url;
        this.formBuilder = formBuilder;
        this.route = route;
        this.actRoute = actRoute;
        this.toastr = toastr;
        this.submitted = false;
        this.isError = false;
        this.loading = true;
        this.isSaving = false;
        this.errorMessage = "";
        //#region User ACL
        this.loadingAcls = true;
        this.isSavingAcls = false;
    }
    ngOnInit() {
        this.loading = true;
        this.userType = new src_app_models_user_type__WEBPACK_IMPORTED_MODULE_2__["UserType"]();
        this.actRoute.paramMap.subscribe(params => {
            this.userType.idUserType = params.get('id');
            this.getUserType();
        });
    }
    getUserType() {
        this.loading = true;
        this.api.postApiData(this.url.userTypeGet(this.userType.idUserType)).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.userType = data.data;
            this.getUserAcls();
            this.userTypeForm = this.formBuilder.group({
                userTypeName: [this.userType.userTypeName, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
                userTypeNum: [this.userType.userTypeNum, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required],
                userTypeSlug: [this.userType.userTypeSlug, _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]
            });
        }, error => {
            this.loading = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong';
        });
    }
    // convenience getter for easy access to form fields
    get f() { return this.userTypeForm.controls; }
    onSubmit() {
        this.submitted = true;
        if (this.userTypeForm.invalid) {
            return;
        }
        this.isError = false;
        this.isSaving = true;
        this.api.putApiData(this.url.userTypeUpdate(this.userType.idUserType), this.userTypeForm.getRawValue()).pipe()
            .subscribe((data) => {
            this.isSaving = false;
            this.submitted = false;
            this.route.navigate(['/user-types/list']);
        }, (error) => {
            this.isSaving = false;
            this.submitted = false;
            this.isError = true;
            this.errorMessage = 'Sorry, something went wrong.';
        });
    }
    getUserAcls() {
        this.userAcls = [];
        this.selectedUserAcls = [];
        this.loadingAcls = true;
        this.api.postApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].userAclList).pipe()
            .subscribe((data) => {
            this.userAcls = data.data;
            let userTypeUserAcls = this.userType.userAcls;
            for (let acl of this.userAcls) {
                acl.isChecked = userTypeUserAcls.find(a => a.idUserAcl == acl.idUserAcl) != null;
            }
            this.loadingAcls = false;
        }, error => {
            this.loadingAcls = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong';
        });
    }
    changeUserAclSelection() {
        this.selectedUserAcls = this.userAcls.filter((value, index) => {
            return value.isChecked;
        });
    }
    saveAcls() {
        let data = {
            acls: this.selectedUserAcls.filter(a => a.isChecked == true).map(m => m.idUserAcl)
        };
        this.isSavingAcls = true;
        this.api.putApiData(src_app_api_urls__WEBPACK_IMPORTED_MODULE_3__["apiUrls"].userTypeSyncAcl(this.userType.idUserType), data).pipe()
            .subscribe((data) => {
            this.isSavingAcls = false;
            this.toastr.success(`${this.userType.userTypeName} synched successfully`, 'Done');
            this.route.navigate(['/user-types/list']);
        }, (error) => {
            this.isSavingAcls = false;
            this.isError = true;
            this.errorMessage = error && error.error.errors.error ? error.error.errors.error : 'Sorry, something went wrong.';
        });
    }
}
