class UserTypeListComponent {
    constructor(auth, urls, dialog, api, toastr) {
        this.auth = auth;
        this.urls = urls;
        this.dialog = dialog;
        this.api = api;
        this.toastr = toastr;
        this.loading = true;
        this.isDeleting = false;
        this.isError = false;
        this.errorMessage = "Sorry, Something went wrong";
        this.selectedUserType = new src_app_models_user_type__WEBPACK_IMPORTED_MODULE_1__["UserType"]();
        this.tableloading = true;
        this.paginationConfig = src_app_constants__WEBPACK_IMPORTED_MODULE_3__["paginationConfig"];
        this.maxsize = 20;
        auth.checkAuth();
    }
    ngOnInit() {
        this.userTypes = [];
        this.getUserTypes();
    }
    pageChanged(event) {
        this.paginationConfig.currentPage = event;
        this.getUserTypes(event);
    }
    getUserTypes(page = 1) {
        this.loading = true;
        let data = {
            page: page,
            pageLength: this.maxsize
        };
        this.api.postApiData(this.urls.userTypeList, data).pipe()
            .subscribe((data) => {
            this.loading = false;
            this.userTypes = data.data;
            if (data.meta.pageLength && data.meta.page) {
                this.paginationConfig.totalItems = data.meta.total;
                this.paginationConfig.currentPage = data.meta.page;
                this.paginationConfig.itemsPerPage = data.meta.pageLength;
                this.paginationConfig.maxSize = data.meta.pageLength;
                this.tableloading = false;
            }
            else {
                this.tableloading = true;
            }
        }, error => {
            this.loading = false;
            this.isError = true;
            this.tableloading = false;
        });
    }
    showDeleteModal(userType) {
        // this.isDeleting = false;
        // let inputElement: HTMLElement = this.deleteModalButton.nativeElement as HTMLElement;
        // inputElement.click();
        const deleteDialog = this.dialog.open(src_app_common_dialog_confirmdialog_confirmdialog_component__WEBPACK_IMPORTED_MODULE_2__["ConfirmdialogComponent"], {
            width: '400px',
            height: '170px',
            data: {
                message: 'Are you sure, you want to delete?'
            }
        });
        deleteDialog.afterClosed().subscribe(data => {
            if (data) {
                this.selectedUserType = userType;
                this.delete();
            }
        });
    }
    delete() {
        if (this.selectedUserType) {
            this.isDeleting = true;
            this.api.putApiData(this.urls.userTypeDelete(this.selectedUserType.idUserType)).pipe()
                .subscribe((data) => {
                this.isDeleting = false;
                // let inputElement: HTMLElement = this.cancelModalButton.nativeElement as HTMLElement;
                // inputElement.click();
                this.toastr.success('UserType deleted successfully', 'Deleted');
                let index = this.userTypes.indexOf(this.selectedUserType);
                if (index >= -1) {
                    this.userTypes.splice(index, 1);
                }
                this.selectedUserType = new src_app_models_user_type__WEBPACK_IMPORTED_MODULE_1__["UserType"]();
            }, error => {
                this.loading = false;
                this.isError = true;
            });
        }
        else {
            alert('User not found');
        }
    }
}
