  /*! no static exports found */

  /***/
  function _(module, exports, __webpack_require__) {
    __webpack_require__(
    /*! /home/abishek/xoomla-admin/node_modules/font-awesome/css/font-awesome.css */
    "./node_modules/font-awesome/css/font-awesome.css");

    __webpack_require__(
    /*! /home/abishek/xoomla-admin/src/styles.scss */
    "./src/styles.scss");

    module.exports = __webpack_require__(
    /*! /home/abishek/xoomla-admin/node_modules/ngx-toastr/toastr.css */
