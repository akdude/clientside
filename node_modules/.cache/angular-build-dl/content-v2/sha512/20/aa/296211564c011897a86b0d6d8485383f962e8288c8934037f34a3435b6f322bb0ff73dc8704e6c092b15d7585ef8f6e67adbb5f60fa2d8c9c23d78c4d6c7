{{item.lable}}</span>\n                                                        </mat-radio-button>\n                                                    </mat-radio-group>\n                                                </div>\n\n                                                <div *ngIf=\"k.paramType=='options'\" class=\"form-group\">\n                                                    <label class=\"control-label\">{{k.title}}<span *ngIf=\"k.isRequired === true\" class=\"c-red\">*</span></label>\n                                                    <select class=\"form-control w-400 custom-op {{k.isRequired ? 'required' : ''}}\" id=\"{{k.inputName}}\">\n                                                        <option *ngFor=\"let l of k.values;\" value=\"{{l.value}}\" [selected]=\"l.selected == 'true'\">{{l.lable}}</option>\n                                                    </select>\n                                                </div>\n\n                                                <div *ngIf=\"k.paramType=='checkbox'\" class=\"form-group\">\n                                                    <label class=\"control-label\">{{k.title}}<span *ngIf=\"k.isRequired === true\" class=\"c-red\">*</span></label>\n                                                    <div *ngFor=\"let item of k.values\">\n                                                        <div *ngIf=\"item.checked == 'true'\">\n                                                            <mat-checkbox class=\"example-margin custom-op\"  id=\"{{k.inputName}}\" [value]=\"item.value\" [checked]=\"\">{{item.lable}}</mat-checkbox>\n                                                        </div>\n                                                        <div *ngIf=\"item.checked == 'false'\">\n                                                            <mat-checkbox class=\"example-margin custom-op\"  id=\"{{k.inputName}}\" [value]=\"item.value\">{{item.lable}}</mat-checkbox>\n                                                        </div>\n                                                    </div>\n                                                </div>\n\n                                                \n                                                <div *ngIf=\"k.paramType=='image'\" class=\"form-group\">\n                                                    <label for=\"categoryImage\" class=\"control-label\">{{k.title}}<span *ngIf=\"k.isRequired === true\" class=\"c-red\">Min 2 required</span></label>\n                                                    <hr class=\"c-hr\">\n                                                    <div *ngFor=\"let item of k.values; index as ik\">\n                                                        <label class=\"control-label\">{{item.lable}}</label>\n                                                        <div id=\"image_html_{{item.name}}\" class=\"mt-20 mb-20 initial_img\">\n                                                            <img *ngIf=\"item.value\" src=\"{{item.value}}\" id=\"mainimg_{{item.name}}\" class=\"c-image final_img edit_img_{{ik}}\">\n                                                            <div *ngIf=\"item.value\" class=\"ml-50 mb-30\">\n                                                                <button type=\"button\" id=\"remove_image_{{item.name}}\" (click)=\"removeImage(item.name)\" class=\"btn btn-danger icon-only ml-15 mt-10\">\n                                                                    <i class=\"fa fa-trash-o\"></i>\n                                                                </button>\n                                                            </div>\n                                                        </div>\n                                                        <label class=\"fileContainer ml-20\">\n                                                            Upload <i class=\"fa fa-upload\" aria-hidden=\"true\"></i>\n                                                