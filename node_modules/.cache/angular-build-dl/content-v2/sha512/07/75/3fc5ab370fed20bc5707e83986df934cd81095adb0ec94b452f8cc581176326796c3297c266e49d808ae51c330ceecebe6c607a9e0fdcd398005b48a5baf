er();\n    ngOnInit(): void {\n      this.getLocations();\n      this.getCategories();\n      this.setForm();\n    }\n\n    ngAfterViewChecked(): void {\n      this.strip();\n    }\n    \n    strip()\n    {\n      var elements = document.getElementsByClassName('parsecontent');\n      if (elements) {\n        for (let i = 0; i < elements.length; i++) {\n          elements[i].innerHTML = (elements[i] as HTMLDivElement).innerText.replace(/(<([^>]+)>)/gi, \"\");\n        }\n      }\n      \n    }\n    setForm(){\n      this.advertismentForm = this.formBuilder.group({\n        idUser: [this.adminUser.get().idUser, Validators.required],\n        idAdCategory: ['', Validators.required],\n        idLocation: ['', Validators.required],\n        adTitle: ['', Validators.required],\n        adType: ['', Validators.required],\n        adDesc: ['', Validators.required],\n        price: ['', Validators.required]\n      });\n    }\n    // convenience getter for easy access to form fields\n    get f() { return this.advertismentForm.controls; }\n  \n    getLocations(){\n      this.loadingLocations = true;\n      this.api.postApiData(apiUrls.locationList).pipe()\n      .subscribe(\n          (data: any) => {\n            this.loadingLocations = false;\n            this.locations = data.data;\n          },\n          (error: any) => {\n            this.errorMessage = 'Sorry, something went wrong.';\n            this.loadingLocations = false;\n            this.isError = true;\n          });\n    }\n\n    getCategories(){\n      this.loadingCategories = true;\n      this.api.postApiData(apiUrls.categoryList).pipe()\n      .subscribe(\n          (data: any) => {\n            this.loadingCategories = false;\n            this.categories = data.data;\n          },\n          (error: any) => {\n            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';\n            this.loadingCategories = false;\n            this.isError = true;\n          });\n    }\n\n    onSelectFile(event,id) { // called each time file input changes\n      if (event.target.files && event.target.files[0]) {\n          var reader = new FileReader();\n          reader.readAsDataURL(event.target.files[0]);\n          reader.onload = (event) => { // called once readAsDataURL is completed\n            var url = event.target.result;\n            var elem = '<img id=\"mainimg_'+id+'\" src = '+url+' class=\"c-image final_img\"></img>'+\n                      '<div class=\" ml-50 mb-30\">'+\n                        '<button\" type=\"button\" id=\"remove_image_'+id+'\" class=\"btn btn-danger icon-only ml-15 mt-10\"><i class=\"fa fa-trash-o\"></i></button>'+\n                      '</div>';\n            var q = document.getElementById('mainimg_'+id);\n            if(q) {\n              q.remove();\n              document.getElementById('remove_image_'+id).remove();\n            }\n            document.getElementById('image_html_'+id).insertAdjacentHTML('afterend' , elem);\n            document.getElementById('remove_image_'+id).addEventListener('click', function() {\n              document.getElementById('mainimg_'+id).remove();\n              document.getElementById('remove_image_'+id).remove();\n            });\n            (document.getElementById(id) as HTMLButtonElement).value = \"\";\n          }\n      }\n    }\n\n    onSubmit(){\n      this.submitted = true;\n      this.isError = false;\n      this.isSaving = true;\n      let data = this.advertismentForm.getRawValue();\n      \n      var initial_images = document.getElementsByClassName('initial_img').length;\n      var final_images = document.getElementsByClassName('final_img').length;\n      if(this.imagerequired && initial_images) {\n        if (final_images <=1) {\n          this.isSaving = false;\n          this.toastr.warning('Please upload atleast 2 images to continue', 'Wait');\n          return;\n        }\n      }\n      if(this.selectedParamOptions.length > 0){\n        for(let item of this.selectedParamOptions) {\n          data[item.paramName] = item.options.join(',');\n        }\n      }\n      var elems = document.getElementsByClassName(\"custom-op\");\n      for (let i = 0; i < elems.length; i++) {\n        if (!elems[i].classList.contains('mat-radio-group') && !elems[i].classList.contains('mat-checkbox')){\n          var id = elems[i].id;\n          var x = (elems[i] as HTMLButtonElement).value;\n          var required = elems[i].classList.contains('required');\n          if(!x && required) {\n            var elem = '<span id=\"error_'+id+'\" style=\"color:brown;\"><i>This field is required</i></span>';\n            var elemexist = document.getElementById('error_'+id);\n            if (!elemexist) {\n              document.getElementById(id).insertAdjacentHTML('afterend' , elem);\n            }\n            this.isSaving = false;\n            this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n            return;\n          } else {\n            if(document.getElementById('error_'+id)) {\n              document.getElementById('error_'+id).remove();\n            }\n          }\n          this.advertismentForm.controls[id].setValue(x);\n          data[id] = x;\n        }\n        if(elems[i].classList.contains('mat-radio-group')) {\n          var rid = elems[i].id;\n          var req = elems[i].classList.contains('required');\n          var rvalue = this.advertismentForm.get(rid).value;\n          if(!rvalue && req){\n            var elem = '<span id=\"error_'+rid+'\" style=\"color:brown;\"><i>This field is required</i></span>';\n            var elemexist = document.getElementById('error_'+rid);\n            if (!elemexist) {\n              document.getElementById(rid).insertAdjacentHTML('afterend' , elem);\n            }\n            this.isSaving = false;\n            this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n            return;\n          } else {\n            this.advertismentForm.controls[rid].setValue(rvalue);\n            if(document.getElementById('error_'+rid)) {\n              document.getElementById('error_'+rid).remove();\n            }\n          }\n        }\n        if ( elems[i].classList.contains('mat-checkbox')) {\n          if (elems[i].classList.contains('mat-checkbox-checked') ) {\n            var value = \"true\";\n          } else {\n            var value = \"false\";\n          }\n          var id = elems[i].id;\n          this.advertismentForm.controls[id].setValue(value);\n          data[id] = value;\n        }\n      }\n      var total_images = document.getElementsByClassName('c-image');\n      for (let s = 0; s < total_images.length; s++) {\n        data[(total_images[s].id).replace('mainimg_','')] = (total_images[s] as HTMLImageElement).src;\n      }\n      if (this.advertismentForm.invalid) {\n        this.isSaving = false;\n        this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n        return;\n      }\n      \n      this.api.postApiData(apiUrls.advertisementCreate, data).pipe()\n      .subscribe(\n          (data: any) => {\n            this.isSaving = false;\n            this.submitted = false;\n            this.toastr.success('Advertisement created successfully', 'Done');\n            this.route.navigate(['/advertisements/list']);\n          },\n          (error: any) => {\n            this.errorMessage = 'Sorry, something went wrong.';\n            this.isSaving = false;\n            this.submitted = false;\n            this.isError = true;\n          });\n    }\n\n    onChangeCategory() {\n      if(this.f.idAdCategory.value){\n        this.loadingCatParams = true;\n        this.api.postApiData(apiUrls.getCategoryParams(this.f.idAdCategory.value), {}).pipe()\n      .subscribe(\n          (res: any) => {\n            this.loadingCatParams = false;\n            this.categoryParams = [];\n            this.categoryParams = res.data;\n            var params = res.data;\n            var existingcontrol = this.allcontrols;\n            if(existingcontrol) {\n              for (let o = 0; o < existingcontrol.length; o++) {\n                const element = existingcontrol[o];\n                this.advertismentForm.removeControl(existingcontrol[o]);\n              }\n            }\n            for (let i = 0; i < params.length; i++) {\n              for (let j = 0; j < params[i]['params'].length; j++) {\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var options = params[i]['params'][j]['values'];\n\n                if(params[i]['params'][j]['paramType'] == 'radio') {\n                  if (isrequired) {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['checked'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                      }\n                    }\n                  } else {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['checked'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'] ));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl(''));\n                      }\n                    }\n                  }\n                  \n                }\n                if(params[i]['params'][j]['paramType'] == 'options') {\n                  if (isrequired) {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['selected'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                      }\n                    }\n                  } else {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['selected'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value']));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl(''));\n                      }\n                    }\n                  }\n                  \n                }\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var isimage = params[i]['params'][j]['paramType'];\n                if (isimage == 'text' || isimage== 'checkbox') {\n                  if (isrequired) {\n                    this.advertismentForm.addControl(element, new FormControl(params[i]['params'][j]['values'][0]['value'], Validators.required));\n                  } else {\n                    this.advertismentForm.addControl(element, new FormControl(''));\n                  }\n                  this.advertismentForm.controls[element].setValue(params[i]['params'][j]['values'][0]['value']);\n                }\n                if(isimage !== 'image'){\n                  this.allcontrols.push(element);\n                }\n              }\n              \n            }\n          },\n          (error: any) => {\n            this.loadingCatParams = false;\n          });\n          \n      }\n    }\n\n    changeParamOptionSelection(event, item, paramName) {\n      for(let option of this.selectedParamOptions){\n        if(option.paramName == paramName){\n          if(event.currentTarget.checked){\n            option.options.push(item.text);\n          } else {\n            option.options.splice(item.text, 1);\n          }\n        }\n      }\n    }\n\n}\n","<div class=\"container-fluid\">\n    <div class=\"row page-title-div\">\n        <div class=\"col-md-6\">\n            <h2 class=\"title\">Create Advertisement</h2>\n        </div>\n    </div>\n    <div class=\"row breadcrumb-div\">\n        <div class=\"col-md-6\">\n            <ul class=\"breadcrumb\">\n\t\t\t\t<li><a [routerLink]=\"'/dashboard'\"><i class=\"fa fa-home\"></i> Home</a></li>\n                <li><a [routerLink]=\"'/advertisements/list'\">Advertisements</a></li>\n\t\t\t\t<li class=\"active\">Post</li>\n\t\t\t</ul>\n        </div>\n    </div>\n</div>\n<section class=\"section\">\n    <div class=\"container-fluid\">\n        <div class=\"row\">\n            <div class=\"col-md-12\">\n                <div class=\"panel\">\n                    <div class=\"panel-heading\">\n                        <div class=\"panel-title\">\n                            <h5>Create Advertisement</h5>\n                        </div>\n                    </div>\n                    <div class=\"panel-body\">\n                        <form [formGroup]=\"advertismentForm\" (ngSubmit)=\"onSubmit()\" id=\"123\">\n                            <div class=\"temp\">\n                                <h3 class=\"ml-15 pl-10 mb-25 c-green\">Ad Details</h3>\n                                <div class=\"col-sm-6\">\n                                    <h6 *ngIf=\"loadingCategories\">Loading categories...<i class=\"fa fa-spinner fa-spin\"></i></h6>\n                                    <div *ngIf=\"!loadingCatego