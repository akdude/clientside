api: ApiDataServices,  \n    private formBuilder: FormBuilder,\n    private route: Router,\n    private actRoute: ActivatedRoute,\n    private toastr: ToastrService) { }\n\n    advertismentForm: FormGroup;\n    submitted = false;\n    isError = false;\n    errorMessage = \"\";\n    adid = \"\";\n    isSaving = false;\n    loading = false;\n    loadingLocations = false;\n    loadingCategories = false;\n    offervalue = false;\n    wantvalue = false;\n    locations: any[];\n    categories: any[];\n    categoryParams: any = [];\n    allcontrols: any = [];\n    mediaFiles: any[] = [];\n    adData: any = [];\n    image_ids: any = [];\n    image_urls: any = [];\n    deletedMediaFiles: MediaFile[] = [];\n    categoryParamOptions = [];\n    fetchedMediaFiles: any[] = [];\n    selectedParamOptions: any[] = [];\n    loadingCatParams = false;\n    adminUser = new AdminUser();\n    advertisement = new Advertisement();\n    ngOnInit(): void {\n      this.getLocations();\n      this.getCategories();\n      this.actRoute.paramMap.subscribe(params => {\n        var ad_id = params.get('id');\n        this.adid = ad_id;\n        this.getAdvertisement(ad_id);\n      });\n      this.setForm();\n    }\n    ngAfterViewChecked(): void {\n      this.strip();\n    }\n    \n    strip()\n    {\n      var elements = document.getElementsByClassName('parsecontent');\n      if (elements) {\n        for (let i = 0; i < elements.length; i++) {\n          elements[i].innerHTML = (elements[i] as HTMLDivElement).innerText.replace(/(<([^>]+)>)/gi, \"\");\n        }\n      }\n      \n    }\n\n    getAdvertisement(ad_id){\n      this.loading = true;\n    this.api.postApiData(apiUrls.advertisementGet(ad_id)).pipe()\n    .subscribe(\n        (data: any) => {\n          let params = data.data.additionalParams;\n          for (let i = 0; i < params.length; i++) {\n            for (let j = 0; j < params[i]['params'].length; j++) {\n              var element = params[i]['params'][j]['inputName'];\n            