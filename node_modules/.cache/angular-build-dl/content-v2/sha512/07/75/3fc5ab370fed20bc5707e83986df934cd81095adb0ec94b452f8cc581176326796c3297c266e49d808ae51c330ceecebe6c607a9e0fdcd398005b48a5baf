files[0]) {\n        var reader = new FileReader();\n        var id = 'cat_image';\n        reader.readAsDataURL(event.target.files[0]);\n        reader.onload = (event) => { // called once readAsDataURL is completed\n          var url = event.target.result;\n          localStorage.setItem('image_url',url.toString());\n          var elem = '<img src = '+url+' class=\"c-image\"></img>';\n            var q = document.getElementById('image_html');\n            if(q && q.nextElementSibling && q.nextElementSibling.classList.contains('c-image')) {\n              q.nextElementSibling.remove();\n            }\n            document.getElementById('image_html').insertAdjacentHTML('afterend' , elem);\n        }\n\n    }\n  }\n\n  addcatparams(i) {\n    var div = document.getElementById('catparam_'+i)as HTMLElement;\n    var elem = (document.getElementById('pageblock_'+i)as HTMLButtonElement);\n    if (elem.value == '') {\n      return;\n    }\n\n    function capitalizeFirstLetter(string) {\n      return string.charAt(0).toUpperCase() + string.slice(1);\n    }\n    \n      var inputtypes = '<select class=\"form-control mt-10 ml-10 c-type option-type\" id=\"input_'+i+'\">'+\n      '                <option>text</option>'+\n      '                <option>radio</option>'+\n      '                <option>options</option>'+\n      '                <option>checkbox</option>'+\n      ((elem.value == 'Media') ? \n      '                <option>image</option>' : '')+\n      '                <option>file</option>'+\n      '            </select>';\n    \n    \n   div.insertAdjacentHTML('beforeend', '<div>'+'<table class=\"table page-div\" id=\"pagediv_'+i+'\" data-pageblock=\"'+elem.value+'\">'+\n    '<th class=\"limegreen\">'+'Category Parameters of Pageblock : '+elem.value+'</th>'+'<button type=\"button\" onclick=\"this.parentElement.remove()\" id=\"custom-close_'+i+'\" class=\"custom-close btn btn-danger\">'+\n    '    <i class=\"fa fa-times\" aria-hidden=\"true\"> </i>'+\n    '</button