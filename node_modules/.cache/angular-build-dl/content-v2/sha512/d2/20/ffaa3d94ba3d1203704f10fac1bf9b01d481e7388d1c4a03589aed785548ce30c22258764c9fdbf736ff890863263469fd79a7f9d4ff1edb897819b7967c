ilder, FormGroup, Validators, FormControl } from '@angular/forms';\nimport { Router, ActivatedRoute } from '@angular/router';\nimport { apiUrls } from 'src/app/api-urls';\nimport { AdminUser } from 'src/app/models/admin-user';\nimport { getMediaFiles } from 'src/app/constants';\nimport { ToastrService } from 'ngx-toastr';\nimport { CategoryParamTypes } from 'src/app/enums';\nimport { Advertisement } from 'src/app/models/advertisement';\nimport { MediaFile } from 'src/app/models/media-file';\nimport * as ClassicEditor from '@ckeditor/ckeditor5-build-classic';\n\n\n@Component({\n  selector: 'app-advertisement-edit',\n  templateUrl: './advertisement-edit.component.html',\n  styleUrls: ['./advertisement-edit.component.css']\n})\nexport class AdvertisementEditComponent implements OnInit {\n  public Editor = ClassicEditor;\n\n  constructor(private api: ApiDataServices,  \n    private formBuilder: FormBuilder,\n    private route: Router,\n    private actRoute: ActivatedRoute,\n    private toastr: ToastrService) { }\n\n    advertismentForm: FormGroup;\n    submitted = false;\n    isError = false;\n    errorMessage = \"\";\n    adid = \"\";\n    isSaving = false;\n    loading = false;\n    loadingLocations = false;\n    loadingCategories = false;\n    offervalue = false;\n    wantvalue = false;\n    locations: any[];\n    categories: any[];\n    categoryParams: any = [];\n    allcontrols: any = [];\n    mediaFiles: any[] = [];\n    adData: any = [];\n    image_ids: any = [];\n    image_urls: any = [];\n    deletedMediaFiles: MediaFile[] = [];\n    categoryParamOptions = [];\n    fetchedMediaFiles: any[] = [];\n    selectedParamOptions: any[] = [];\n    loadingCatParams = false;\n    adminUser = new AdminUser();\n    advertisement = new Advertisement();\n    ngOnInit(): void {\n      this.getLocations();\n      this.getCategories();\n      this.actRoute.paramMap.subscribe(params => {\n        var ad_id = params.get('id');\n        this.adid = ad_id;\n        this.getAdvertisement(ad_id);\n      });\n      this.setForm();\n    }\n    ngAfterViewChecked(): void {\n      this.strip();\n    }\n    \n    strip()\n    {\n      var elements = document.getElementsByClassName('parsecontent');\n      if (elements) {\n        for (let i = 0; i < elements.length; i++) {\n          elements[i].innerHTML = (elements[i] as HTMLDivElement).innerText.replace(/(<([^>]+)>)/gi, \"\");\n        }\n      }\n      \n    }\n\n    getAdvertisement(ad_id){\n      this.loading = true;\n    this.api.postApiData(apiUrls.advertisementGet(ad_id)).pipe()\n    .subscribe(\n        (data: any) => {\n          let params = data.data.additionalParams;\n          for (let i = 0; i < params.length; i++) {\n            for (let j = 0; j < params[i]['params'].length; j++) {\n              var element = params[i]['params'][j]['inputName'];\n              var isrequired = params[i]['params'][j]['isRequired'];\n              var isimage = params[i]['params'][j]['paramType'];\n              if(params[i]['params'][j]['paramType'] == 'radio') {\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var options = params[i]['params'][j]['values'];\n                if (isrequired) {\n                  for (let z = 0; z < options.length; z++) {\n                    if (options[z]['checked'] == \"true\") {\n                      this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                      this.advertismentForm.controls[element].setValue(options[z]['value']);\n                    } else {\n                      this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                    }\n                  }\n                } else {\n                  for (let z = 0; z < options.length; z++) {\n                    if (options[z]['checked'] == \"true\") {\n                      this.advertismentForm.addControl(element, new FormControl(options[z]['value'] ));\n                      this.advertismentForm.controls[element].setValue(options[z]['value']);\n                    } else {\n                      this.advertismentForm.addControl(element, new FormControl(''));\n                    }\n                  }\n                }\n                \n              }\n              if(params[i]['params'][j]['paramType'] == 'options') {\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var options = params[i]['params'][j]['values'];\n                if (isrequired) {\n                  for (let z = 0; z < options.length; z++) {\n                    if (options[z]['selected'] == \"true\") {\n                      this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                      this.advertismentForm.controls[element].setValue(options[z]['value']);\n                    } else {\n                      this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                    }\n                  }\n                } else {\n                  for (let z = 0; z < options.length; z++) {\n                    if (options[z]['selected'] == \"true\") {\n                      this.advertismentForm.addControl(element, new FormControl(options[z]['value']));\n                      this.advertismentForm.controls[element].setValue(options[z]['value']);\n                    } else {\n                      this.advertismentForm.addControl(element, new FormControl(''));\n                    }\n                  }\n                }\n                \n              }\n              if (isimage == 'text' || isimage== 'checkbox') {\n                if (isrequired) {\n                  this.advertismentForm.addControl(element, new FormControl(params[i]['params'][j]['values'][0]['value'], Validators.required));\n                } else {\n                  this.advertismentForm.addControl(element, new FormControl(''));\n                }\n                this.advertismentForm.controls[element].setValue(params[i]['params'][j]['values'][0]['value']);\n              }\n              if(isimage !== 'image'){\n                this.allcontrols.push(element);\n              }\n            }\n            \n          }\n          for (let i = 0; i < params.length; i++) {\n            if(params[i].block == 'Media') {\n              var mediablockparams = params[i].params; \n              for (let j = 0; j < mediablockparams.length; j++) {\n                if(mediablockparams[j].paramType == 'image'){\n                  var images = mediablockparams[j].values;\n                  for (let k = 0; k < images.length; k++) {\n                    this.mediaFiles[k] = [images[k].name,images[k].value,images[k].idAdGallery];\n                  }\n                }\n              }\n            }\n\n          }\n          this.loading = false;\n          \n          this.categoryParams = data.data.additionalParams;\n          this.advertismentForm.patchValue({'idAdCategory' : data.data.idAdCategory});\n          this.advertismentForm.patchValue({'adTitle' : data.data.adTitle});\n          this.advertismentForm.patchValue({'adDesc' : data.data.adDesc});\n          if(data.data.adType == 'Offering') {\n            this.offervalue = true;\n            this.wantvalue = false;\n          } else {\n            this.offervalue = false;\n            this.wantvalue = true;\n          }\n          (document.getElementById('advertiser') as HTMLButtonElement).value = data.data.advertiserType;\n          this.advertismentForm.patchValue({'price' : data.data.price});\n          this.advertismentForm.patchValue({'idLocation' : data.data.idLocation});\n        },\n        error => {\n          this.loading = false;\n          this.isError = true;\n          this.errorMessage = 'Sorry, something went wrong';\n        });\n    }\n  \n    setForm(){\n      this.advertismentForm = this.formBuilder.group({\n        idUser: [this.adminUser.get().idUser, Validators.required],\n        idAdCategory: ['', Validators.required],\n        idLocation: ['', Validators.required],\n        adTitle: ['', Validators.required],\n        adDesc: ['', Validators.required],\n        price: ['', Validators.required],\n        adType: [(this.offervalue) ? (this.offervalue) : (this.wantvalue), Validators.required]\n      });\n    }\n    // convenience getter for easy access to form fields\n    get f() { return this.advertismentForm.controls; }\n  \n    getLocations(){\n      this.loadingLocations = true;\n      this.api.postApiData(apiUrls.locationList).pipe()\n      .subscribe(\n          (data: any) => {\n            this.loadingLocations = false;\n            this.locations = data.data;\n          },\n          (error: any) => {\n            this.errorMessage = 'Sorry, something went wrong.';\n            this.loadingLocations = false;\n            this.isError = true;\n          });\n    }\n\n    getCategories(){\n      this.loadingCategories = true;\n      this.api.postApiData(apiUrls.categoryList).pipe()\n      .subscribe(\n          (data: any) => {\n            this.loadingCategories = false;\n            this.categories = data.data;\n          },\n          (error: any) => {\n            this.errorMessage = error && error.message ? error.message : 'Sorry, something went wrong.';\n            this.loadingCategories = false;\n            this.isError = true;\n          });\n    }\n\n    onSelectFile(event,id,count) { // called each time file input changes\n      if (event.target.files && event.target.files[0]) {\n          var reader = new FileReader();\n          reader.readAsDataURL(event.target.files[0]);\n          reader.onload = (event) => { // called once readAsDataURL is completed\n            var url = event.target.result;\n            this.image_ids.push(id);\n            this.image_urls.push(url.toString());\n            var elem = '<img id=\"mainimg_'+id+'\" src = '+url+' class=\"c-image final_img edit_img_'+count+'\"></img>'+\n                      '<div class=\" ml-50 mb-30\">'+\n                        '<button\" type=\"button\" id=\"remove_image_'+id+'\" class=\"btn btn-danger icon-only ml-15 mt-10\"><i class=\"fa fa-trash-o\"></i></button>'+\n                      '</div>';\n            var q = document.getElementById('mainimg_'+id);\n            if(q) {\n              q.remove();\n              document.getElementById('remove_image_'+id).remove();\n            }\n            document.getElementById('image_html_'+id).insertAdjacentHTML('afterend' , elem);\n            document.getElementById('remove_image_'+id).addEventListener('click', function() {\n              document.getElementById('mainimg_'+id).remove();\n              document.getElementById('remove_image_'+id).remove();\n            });\n            (document.getElementById(id) as HTMLButtonElement).value = \"\";\n          }\n  \n      }\n    }\n    removeImage(id) {\n      document.getElementById('mainimg_'+id).remove();\n              document.getElementById('remove_image_'+id).remove();\n              (document.getElementById(id) as HTMLButtonElement).value = \"\";\n    }\n\n    onSubmit(){\n      this.submitted = true;\n      this.isError = false;\n      this.isSaving = true;\n      let data = this.advertismentForm.getRawValue();\n      if(this.selectedParamOptions.length > 0){\n        for(let item of this.selectedParamOptions) {\n          data[item.paramName] = item.options.join(',');\n        }\n      }\n      var elems = document.getElementsByClassName(\"custom-op\");\n      for (let i = 0; i < elems.length; i++) {\n        if (!elems[i].classList.contains('mat-radio-group') && !elems[i].classList.contains('mat-checkbox')){\n          var id = elems[i].id;\n          var x = (elems[i] as HTMLButtonElement).value;\n          var required = elems[i].classList.contains('required');\n          if(!x && required) {\n            var elem = '<span id=\"error_'+id+'\" style=\"color:brown;\"><i>This field is required</i></span>';\n            var elemexist = document.getElementById('error_'+id);\n            if (!elemexist) {\n              document.getElementById(id).insertAdjacentHTML('afterend' , elem);\n            }\n            this.isSaving = false;\n            this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n            return;\n          } else {\n            if(document.getElementById('error_'+id)) {\n              document.getElementById('error_'+id).remove();\n            }\n          }\n          this.advertismentForm.controls[id].setValue(x);\n\n          data[id] = x;\n        }\n\n        if(elems[i].classList.contains('mat-radio-group')) {\n          var rid = elems[i].id;\n          var req = elems[i].classList.contains('required');\n          var rvalue = this.advertismentForm.get(rid).value;\n          if(!rvalue && req){\n            var elem = '<span id=\"error_'+rid+'\" style=\"color:brown;\"><i>This field is required</i></span>';\n            var elemexist = document.getElementById('error_'+rid);\n            if (!elemexist) {\n              document.getElementById(rid).insertAdjacentHTML('afterend' , elem);\n            }\n            this.isSaving = false;\n            this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n            return;\n          } else {\n            this.advertismentForm.controls[rid].setValue(rvalue);\n            if(document.getElementById('error_'+rid)) {\n              document.getElementById('error_'+rid).remove();\n            }\n          }\n        }\n\n        if ( elems[i].classList.contains('mat-checkbox')) {\n          if (elems[i].classList.contains('mat-checkbox-checked') ) {\n            var value = \"true\";\n          } else {\n            var value = \"false\";\n          }\n          var id = elems[i].id;\n          this.advertismentForm.controls[id].setValue(value);\n          data[id] = value;\n        }\n      }\n\n      var radio = document.getElementsByClassName(\"mat-radio-group\");\n      if (radio) {\n        for (let c = 0; c < radio.length; c++) {\n          var id = radio[c].id;\n          for (let i = 0; i < radio[c].childElementCount; i++) {\n            if (radio[c].children[i].classList.contains('mat-radio-checked')) {\n              data[id] = radio[c].children[i].attributes[2].value;\n            }\n            \n          }\n        }\n      }\n      var mediafiles = this.mediaFiles;\n      for (let l = 0; l < mediafiles.length; l++) {\n        var imageid = mediafiles[l][0];\n        data[imageid] = {};\n        var element = document.getElementsByClassName('edit_img_'+[l]);\n        if(element[0]) {\n          var src = (element[0] as HTMLImageElement).src;\n          if (src == mediafiles[l][1]) {\n            data[imageid].image = mediafiles[l][1];\n            data[imageid].idAdGallery = mediafiles[l][2];//same data with filled images\n          } else {\n            var oldelem = mediafiles[l][1];\n            if (oldelem) {\n              data[imageid].image = src;\n              data[imageid].idAdGallery = mediafiles[l][2];//update\n            } else {\n              data[imageid].image = src;\n              data[imageid].idAdGallery = \"\";//create\n            }\n          }\n\n        } else {\n          var exist = mediafiles[l][1];\n          if (exist) {\n            data[imageid].image = \"\";\n            data[imageid].idAdGallery = mediafiles[l][2];//delete\n          } else {\n            data[imageid].image = \"\";\n            data[imageid].idAdGallery = \"\";//same data with no images\n          }\n        }\n        \n      }\n      if (this.advertismentForm.invalid) {\n        this.isSaving = false;\n        this.toastr.warning('Please fill in all the fields to continue', 'Wait');\n        return;\n      }\n      this.api.putApiData(apiUrls.advertisementUpdate(this.adid), data).pipe()\n      .subscribe(\n          (data: any) => {\n            this.isSaving = false;\n            this.submitted = false;\n            this.toastr.success('Advertisement updated successfully', 'Done');\n            this.route.navigate(['/advertisements/list']);\n          },\n          (error: any) => {\n            this.errorMessage = 'Sorry, something went wrong.';\n            this.isSaving = false;\n            this.submitted = false;\n            this.isError = true;\n          });\n    }\n    deleteMedial(idAdGallery) {\n      let media = this.mediaFiles.find(a => a.idAdGallery == idAdGallery);\n      if(media) {\n        this.mediaFiles.splice(this.mediaFiles.findIndex(a => a.idAdGallery == idAdGallery), 1);\n        this.deletedMediaFiles.push(media);\n      }\n    }\n\n    \n    onChangeCategory() {\n      if(this.f.idAdCategory.value){\n        this.loadingCatParams = true;\n        this.api.postApiData(apiUrls.getCategoryParams(this.f.idAdCategory.value), {}).pipe()\n      .subscribe(\n          (res: any) => {\n            this.loadingCatParams = false;\n            this.categoryParams = [];\n            this.categoryParams = res.data;\n            var params = res.data;\n            var existingcontrol = this.allcontrols;\n            for (let o = 0; o < existingcontrol.length; o++) {\n              const element = existingcontrol[o];\n              this.advertismentForm.removeControl(existingcontrol[o]);\n            }\n            for (let i = 0; i < params.length; i++) {\n              for (let j = 0; j < params[i]['params'].length; j++) {\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var options = params[i]['params'][j]['values'];\n\n                if(params[i]['params'][j]['paramType'] == 'radio') {\n                  if (isrequired) {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['checked'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                      }\n                    }\n                  } else {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['checked'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'] ));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl(''));\n                      }\n                    }\n                  }\n                  \n                }\n                if(params[i]['params'][j]['paramType'] == 'options') {\n                  if (isrequired) {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['selected'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value'], Validators.required));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl('', Validators.required));\n                      }\n                    }\n                  } else {\n                    for (let z = 0; z < options.length; z++) {\n                      if (options[z]['selected'] == \"true\") {\n                        this.advertismentForm.addControl(element, new FormControl(options[z]['value']));\n                        this.advertismentForm.controls[element].setValue(options[z]['value']);\n                      } else {\n                        this.advertismentForm.addControl(element, new FormControl(''));\n                      }\n                    }\n                  }\n                  \n                }\n                var element = params[i]['params'][j]['inputName'];\n                var isrequired = params[i]['params'][j]['isRequired'];\n                var isimage = params[i]['params'][j]['paramType'];\n                if (isimage == 'text' || isimage== 'checkbox') {\n                  if (isrequired) {\n                    this.advertismentForm.addControl(element, new FormControl(params[i]['params'][j]['values'][0]['value'], Validators.required));\n                  } else {\n                    this.advertismentForm.addControl(element, new FormControl(''));\n                  }\n                  this.advertismentForm.controls[element].setValue(params[i]['params'][j]['values'][0]['value']);\n                }\n                if(isimage !== 'image'){\n                  this.allcontrols.push(element);\n                }\n              }\n              \n            }\n          },\n          (error: any) => {\n            this.loadingCatParams = false;\n          });\n          \n      }\n    }\n\n    changeParamOptionSelection(event, item, paramName) {\n      for(let option of this.selectedParamOptions){\n        if(option.paramName == paramName){\n          if(event.currentTarget.checked){\n            option.options.push(item.text);\n          } else {\n            option.options.splice(item.text, 1);\n          }\n        }\n      }\n    }\n\n}\n","<div class=\"container-fluid\">\n    <div class=\"row page-title-div\">\n        <div class=\"col-md-6\">\n            <h2 class=\"title\">Edit Advertisement</h2>\n        </div>\n    </div>\n    <div class=\"row breadcrumb-div\">\n        <div class=\"col-md-6\">\n            <ul class