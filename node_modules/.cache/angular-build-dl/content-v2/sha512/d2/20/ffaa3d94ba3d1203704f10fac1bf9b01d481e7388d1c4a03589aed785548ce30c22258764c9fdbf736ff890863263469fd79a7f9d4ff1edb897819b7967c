ut-' + k;\n      };\n    }\n\n    var abcElements = document.querySelectorAll('.input-div');\n    for (var k = 0; k < abcElements.length; k++)\n    {\n      if(abcElements[k].id !== 'inputdiv-' + k) {\n        abcElements[k].id = 'inputdiv-' + k;\n        gen(k);\n      }\n    }\n    \n    function gen(k)\n    {\n      var elem2 = (document.getElementById('inputdiv-'+k)as HTMLButtonElement);\n      if (elem2) {\n        elem2.addEventListener('click', function(){\n          var final_div = document.getElementById('finaldiv-'+k)as HTMLElement;\n          var input_type = (document.getElementById('input-'+k)as HTMLButtonElement).value;\n          var pageblock_type = (document.getElementById('pagediv-'+k)as HTMLElement).dataset.pageblock;\n          if ((input_type !== 'radio') && (input_type !== 'options')) {\n              var text_default = '<td>'+\n              '    Placeholder'+\n              '    <input type=\"text\" id=\"default_'+i+'\" required=\"\" placeholder=\"Values seperated with comma\" class=\"form-control\">'+\n              '</td>';\n\n              var check_default = '<td class=\"pt-0\">'+\n              '    Selected'+\n              '<select required=\"\" id=\"default_'+i+'\" class=\"form-control w-70\">'+\n              '        <option value=\"true\">True</option>'+\n              '        <option value=\"false\">False</option>'+\n              '    </select>'+\n              '</td>';\n              if(input_type == 'checkbox') {\n                var default_div = check_default;\n              } else {\n                var default_div = text_default;\n              }\n              if(input_type == 'image') {\n                var otid = 'pageblock_image';\n              } else {\n                var otid = \"\";\n              }\n              var exist = document.getElementsByClassName('pageblock_image');\n              if(otid && exist.length > 0) {\n                return;\n              }\n              \n              final_div.insertAdja