import { EventEmitter } from '@angular/core';
import { SweetAlertOptions, SweetAlertType } from 'sweetalert2';
export declare class SwalComponent {
    private defaultSwalOptions;
    type: SweetAlertType;
    title: string;
    text: string;
    html: string;
    options: SweetAlertOptions;
    confirm: EventEmitter<any>;
    cancel: EventEmitter<any>;
    constructor(defaultSwalOptions: SweetAlertOptions);
    show(): Promise<any>;
}
